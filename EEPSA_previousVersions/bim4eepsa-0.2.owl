        <rdfs:comment xml:lang="en">Main fire protection class for the building which is assigned from the fire protection classification table as given by the relevant national building code.</rdfs:comment>
        <rdfs:label xml:lang="en">Fire protection class</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/bim4eepsa#pset_BuildingCommon_grossPlannedArea -->

    <owl:DatatypeProperty rdf:about="https://w3id.org/bim4eepsa#pset_BuildingCommon_grossPlannedArea">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#double"/>
        <rdfs:comment xml:lang="en">Total planned gross area for the building Used for programming the building.</rdfs:comment>
        <rdfs:label xml:lang="en">Gross planned area</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/bim4eepsa#pset_BuildingCommon_netPlannedArea -->

    <owl:DatatypeProperty rdf:about="https://w3id.org/bim4eepsa#pset_BuildingCommon_netPlannedArea">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#double"/>
