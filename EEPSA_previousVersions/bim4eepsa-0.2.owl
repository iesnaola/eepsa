        <rdfs:label xml:lang="en">Net planned area</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/bim4eepsa#pset_BuildingCommon_numberOfStoreys -->

    <owl:DatatypeProperty rdf:about="https://w3id.org/bim4eepsa#pset_BuildingCommon_numberOfStoreys">
