                <owl:onProperty rdf:resource="http://w3id.org/eepsa#isSpaceOf"/>
                <owl:allValuesFrom>
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="http://w3id.org/eepsa#Building"/>
                            <rdf:Description rdf:about="http://w3id.org/eepsa#BuildingSpace"/>
                        </owl:unionOf>
                    </owl:Class>
                </owl:allValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://w3id.org/eepsa#pset_isExternal"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onDataRange rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>A space represents an area or volume bounded actually or theoretically. Spaces are areas or volumes that provide for certain functions within a building.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://ifcowl.openbimstandards.org/IFC4_ADD2#IfcSpace"/>
    </owl:Class>
    


    <!-- http://w3id.org/eepsa#DeviceRelocation -->

    <owl:Class rdf:about="http://w3id.org/eepsa#DeviceRelocation">
        <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Method"/>
        <rdfs:comment xml:lang="en">A method that consists in placing the sensing device in an adequate location. Following guides can be followed:

https://www.rmets.org/sites/default/files/pdf/guidelines/aws-guide.pdf

http://weatherfaqs.org.uk/node/124

http://www.conservationphysics.org/datalog/datlog6.php

http://www.weatherstations.co.uk/gooddata.htm</rdfs:comment>
    </owl:Class>
    


    <!-- http://w3id.org/eepsa#DeviceShelter -->

    <owl:Class rdf:about="http://w3id.org/eepsa#DeviceShelter">
        <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Method"/>
        <rdfs:comment xml:lang="en">A method that consists in covering the sensing device adequately to avoid erroneous observations. For example, Stevenson Screen to shield the sensing device against direct precipitation or direct sunlight.</rdfs:comment>
    </owl:Class>
    


    <!-- http://w3id.org/eepsa#Door -->

    <owl:Class rdf:about="http://w3id.org/eepsa#Door">
        <rdfs:subClassOf rdf:resource="http://w3id.org/eepsa#BuildingElement"/>
        <owl:disjointWith rdf:resource="http://w3id.org/eepsa#Wall"/>
        <owl:disjointWith rdf:resource="http://w3id.org/eepsa#Window"/>
        <rdfs:comment>Definition according to ISO 6707-1: construction for closing an opening, intended primarily for access with hinged, pivoted or sliding operation.</rdfs:comment>
        <rdfs:seeAlso rdf:resource="http://ifcowl.openbimstandards.org/IFC4_ADD2#IfcDoor"/>
    </owl:Class>
    


    <!-- http://w3id.org/eepsa#EastOrientation -->

    <owl:Class rdf:about="http://w3id.org/eepsa#EastOrientation">
        <rdfs:subClassOf rdf:resource="http://w3id.org/eepsa#Orientation"/>
    </owl:Class>
    


    <!-- http://w3id.org/eepsa#ExternalBuildingElement -->

    <owl:Class rdf:about="http://w3id.org/eepsa#ExternalBuildingElement">
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://w3id.org/eepsa#pset_isExternal"/>
                <owl:hasValue rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</owl:hasValue>
            </owl:Restriction>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://w3id.org/eepsa#BuildingElement"/>
        <rdfs:comment xml:lang="en">Building elements that are external (face the outside of the building)</rdfs:comment>
    </owl:Class>
    


    <!-- http://w3id.org/eepsa#ExternalDoor -->

    <owl:Class rdf:about="http://w3id.org/eepsa#ExternalDoor">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://w3id.org/eepsa#Door"/>
                    <rdf:Description rdf:about="http://w3id.org/eepsa#ExternalBuildingElement"/>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://w3id.org/eepsa#ExternalBuildingElement"/>
        <rdfs:comment xml:lang="en">External door (faces the outside of the building)</rdfs:comment>
    </owl:Class>
    


    <!-- http://w3id.org/eepsa#ExternalWall -->

    <owl:Class rdf:about="http://w3id.org/eepsa#ExternalWall">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://w3id.org/eepsa#ExternalBuildingElement"/>
                    <rdf:Description rdf:about="http://w3id.org/eepsa#Wall"/>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://w3id.org/eepsa#Wall"/>
        <rdfs:comment xml:lang="en">External wall (faces the outside of the building)</rdfs:comment>
    </owl:Class>
    


    <!-- http://w3id.org/eepsa#ExternalWindow -->

    <owl:Class rdf:about="http://w3id.org/eepsa#ExternalWindow">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://w3id.org/eepsa#ExternalBuildingElement"/>
                    <rdf:Description rdf:about="http://w3id.org/eepsa#Window"/>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://w3id.org/eepsa#Window"/>
        <rdfs:comment xml:lang="en">External window (faces the outside of the building)</rdfs:comment>
    </owl:Class>
    


    <!-- http://w3id.org/eepsa#HVACActuator -->

    <owl:Class rdf:about="http://w3id.org/eepsa#HVACActuator">
        <rdfs:subClassOf rdf:resource="http://www.irit.fr/recherches/MELODI/ontologies/SAN#ActuatingDevice"/>
        <rdfs:comment xml:lang="en">HVAC actuator for cooling or heating</rdfs:comment>
    </owl:Class>
    


    <!-- http://w3id.org/eepsa#Indoor -->

    <owl:Class rdf:about="http://w3id.org/eepsa#Indoor">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/2003/01/geo/wgs84_pos#SpatialThing"/>
        <rdfs:comment xml:lang="en">Happening, used, or existing inside a building (by http://dictionary.cambridge.org/dictionary/english/indoor)</rdfs:comment>
    </owl:Class>
    


    <!-- http://w3id.org/eepsa#IndoorRelativeHumidity -->

    <owl:Class rdf:about="http://w3id.org/eepsa#IndoorRelativeHumidity">
        <rdfs:subClassOf rdf:resource="http://purl.org/iot/vocab/m3-lite#RelativeHumidity"/>
    </owl:Class>
    


    <!-- http://w3id.org/eepsa#IndoorTemperature -->

    <owl:Class rdf:about="http://w3id.org/eepsa#IndoorTemperature">
        <rdfs:subClassOf rdf:resource="http://purl.org/iot/vocab/m3-lite#Temperature"/>
    </owl:Class>
    


    <!-- http://w3id.org/eepsa#LightActuator -->

    <owl:Class rdf:about="http://w3id.org/eepsa#LightActuator">
        <rdfs:subClassOf rdf:resource="http://www.irit.fr/recherches/MELODI/ontologies/SAN#ActuatingDevice"/>
        <rdfs:comment xml:lang="en">Actuator used to turn the lights on or off.</rdfs:comment>
    </owl:Class>
    


    <!-- http://w3id.org/eepsa#MeteorologicalProperty -->

    <owl:Class rdf:about="http://w3id.org/eepsa#MeteorologicalProperty">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/ssn/Property"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://w3id.org/eepsa#hasDataSource"/>
                <owl:someValuesFrom rdf:resource="http://w3id.org/eepsa#WeatherService"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">Meteorological Variables that can be measured by weather stations (this means that they are outdoors. Maybe the name has to be changed)</rdfs:comment>
    </owl:Class>
    


    <!-- http://w3id.org/eepsa#NaturallyEnlightenedSpace -->

    <owl:Class rdf:about="http://w3id.org/eepsa#NaturallyEnlightenedSpace">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://w3id.org/eepsa#BuildingSpace"/>
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="http://w3id.org/eepsa#containsBuildingElement"/>
                                <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minQualifiedCardinality>
                                <owl:onClass rdf:resource="http://w3id.org/eepsa#ExternalWindow"/>
                            </owl:Restriction>
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="http://w3id.org/eepsa#containsBuildingElement"/>
                                <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minQualifiedCardinality>
                                <owl:onClass rdf:resource="http://w3id.org/eepsa#Skylight"/>
                            </owl:Restriction>
                        </owl:unionOf>
                    </owl:Class>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://w3id.org/eepsa#BuildingSpace"/>
        <rdfs:subClassOf>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://w3id.org/eepsa#isHighlyAffectedBy"/>
                        <owl:hasValue rdf:resource="http://purl.org/iot/vocab/m3-lite#CloudCover"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://w3id.org/eepsa#isHighlyAffectedBy"/>
                        <owl:hasValue rdf:resource="http://purl.org/iot/vocab/m3-lite#SolarRadiation"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://w3id.org/eepsa#isHighlyAffectedBy"/>
                        <owl:hasValue rdf:resource="http://purl.org/iot/vocab/m3-lite#SunPositionDirection"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://w3id.org/eepsa#isHighlyAffectedBy"/>
                        <owl:hasValue rdf:resource="http://purl.org/iot/vocab/m3-lite#SunPositionElevation"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://w3id.org/eepsa#NorthOrientation -->

    <owl:Class rdf:about="http://w3id.org/eepsa#NorthOrientation">
        <rdfs:subClassOf rdf:resource="http://w3id.org/eepsa#Orientation"/>
    </owl:Class>
    


    <!-- http://w3id.org/eepsa#Orientation -->

    <owl:Class rdf:about="http://w3id.org/eepsa#Orientation">
        <rdfs:comment xml:lang="en">The direction an envelope element faces, i.e., the direction of a vector perpendicular to and pointing away from the surface outside of the element (Building Energy Codes Program).</rdfs:comment>
    </owl:Class>
    


    <!-- http://w3id.org/eepsa#Outdoor -->

    <owl:Class rdf:about="http://w3id.org/eepsa#Outdoor">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/2003/01/geo/wgs84_pos#SpatialThing"/>
        <rdfs:comment xml:lang="en">Existing, happening, or done outside, rather than inside a building: (http://dictionary.cambridge.org/dictionary/english/outdoor)</rdfs:comment>
    </owl:Class>
    


    <!-- http://w3id.org/eepsa#OutdoorRelativeHumidity -->

    <owl:Class rdf:about="http://w3id.org/eepsa#OutdoorRelativeHumidity">
        <rdfs:subClassOf rdf:resource="http://purl.org/iot/vocab/m3-lite#RelativeHumidity"/>
        <rdfs:subClassOf rdf:resource="http://w3id.org/eepsa#MeteorologicalProperty"/>
    </owl:Class>
    


    <!-- http://w3id.org/eepsa#OutdoorTemperature -->

    <owl:Class rdf:about="http://w3id.org/eepsa#OutdoorTemperature">
        <rdfs:subClassOf rdf:resource="http://purl.org/iot/vocab/m3-lite#Temperature"/>
        <rdfs:subClassOf rdf:resource="http://w3id.org/eepsa#MeteorologicalProperty"/>
    </owl:Class>
    


    <!-- http://w3id.org/eepsa#Outlier -->

    <owl:Class rdf:about="http://w3id.org/eepsa#Outlier">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/ssn/Observation"/>
        <rdfs:comment>Observations that stand out and do not conform to the expected behaviour</rdfs:comment>
    </owl:Class>
    


    <!-- http://w3id.org/eepsa#OutlierCausedByBeamSpotlight -->

    <owl:Class rdf:about="http://w3id.org/eepsa#OutlierCausedByBeamSpotlight">
        <rdfs:subClassOf rdf:resource="http://w3id.org/eepsa#OutlierCausedByDeviceLocation"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://w3id.org/eepsa#hasSolvingMethod"/>
                <owl:allValuesFrom rdf:resource="http://w3id.org/eepsa#DeviceRelocation"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">An erroneous observation (e.g. temperature, illuminance) caused by a beam spotlight. For example, an observation made by a sensing device located under a light bulb</rdfs:comment>
    </owl:Class>
    


    <!-- http://w3id.org/eepsa#OutlierCausedByDeviceError -->

    <owl:Class rdf:about="http://w3id.org/eepsa#OutlierCausedByDeviceError">
        <rdfs:subClassOf rdf:resource="http://w3id.org/eepsa#Outlier"/>
        <rdfs:comment>Outliers caused by an error on the device itself</rdfs:comment>
    </owl:Class>
    


    <!-- http://w3id.org/eepsa#OutlierCausedByDeviceLocation -->

    <owl:Class rdf:about="http://w3id.org/eepsa#OutlierCausedByDeviceLocation">
        <rdfs:subClassOf rdf:resource="http://w3id.org/eepsa#Outlier"/>
    </owl:Class>
    


    <!-- http://w3id.org/eepsa#OutlierCausedByDeviceLowBattery -->

    <owl:Class rdf:about="http://w3id.org/eepsa#OutlierCausedByDeviceLowBattery">
        <rdfs:subClassOf rdf:resource="http://w3id.org/eepsa#OutlierCausedByDeviceError"/>
    </owl:Class>
    


    <!-- http://w3id.org/eepsa#OutlierCausedByDeviceMalfunction -->

    <owl:Class rdf:about="http://w3id.org/eepsa#OutlierCausedByDeviceMalfunction">
        <rdfs:subClassOf rdf:resource="http://w3id.org/eepsa#OutlierCausedByDeviceError"/>
        <rdfs:comment xml:lang="en">An erroneous observation caused by a malfunction suffered by the device, which might have been transient.</rdfs:comment>
    </owl:Class>
    


    <!-- http://w3id.org/eepsa#OutlierCausedByExternalConditions -->

    <owl:Class rdf:about="http://w3id.org/eepsa#OutlierCausedByExternalConditions">
        <rdfs:subClassOf rdf:resource="http://w3id.org/eepsa#OutlierCausedByDeviceLocation"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://w3id.org/eepsa#hasSolvingMethod"/>
                <owl:allValuesFrom rdf:resource="http://w3id.org/eepsa#DeviceRelocation"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">An erroneous observation (e.g. temperature, illuminance) caused by external factors. For example for being next to a source of radiant heat or cold such as windows and poorly insulated outside walls.</rdfs:comment>
    </owl:Class>
    


    <!-- http://w3id.org/eepsa#OutlierCausedByHVAC -->

    <owl:Class rdf:about="http://w3id.org/eepsa#OutlierCausedByHVAC">
        <rdfs:subClassOf rdf:resource="http://w3id.org/eepsa#OutlierCausedByDeviceLocation"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://w3id.org/eepsa#hasSolvingMethod"/>
                <owl:allValuesFrom rdf:resource="http://w3id.org/eepsa#DeviceRelocation"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">An erroneous observation (e.g. temperature) caused by its placement next to an activated HVAC system (whether it was heating or cooling)</rdfs:comment>
    </owl:Class>
    


    <!-- http://w3id.org/eepsa#OutlierCausedByRain -->

    <owl:Class rdf:about="http://w3id.org/eepsa#OutlierCausedByRain">
        <rdfs:subClassOf rdf:resource="http://w3id.org/eepsa#OutlierCausedByDeviceLocation"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://w3id.org/eepsa#hasSolvingMethod"/>
                <owl:allValuesFrom rdf:resource="http://w3id.org/eepsa#DeviceRelocation"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://w3id.org/eepsa#hasSolvingMethod"/>
                <owl:allValuesFrom rdf:resource="http://w3id.org/eepsa#DeviceShelter"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">An erroneous observation caused by rainfall.</rdfs:comment>
    </owl:Class>
    


    <!-- http://w3id.org/eepsa#OutlierCausedBySunlight -->

    <owl:Class rdf:about="http://w3id.org/eepsa#OutlierCausedBySunlight">
        <rdfs:subClassOf rdf:resource="http://w3id.org/eepsa#OutlierCausedByDeviceLocation"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://w3id.org/eepsa#hasSolvingMethod"/>
                <owl:allValuesFrom rdf:resource="http://w3id.org/eepsa#DeviceRelocation"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://w3id.org/eepsa#hasSolvingMethod"/>
                <owl:allValuesFrom rdf:resource="http://w3id.org/eepsa#DeviceShelter"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">An erroneous observation (e.g. temperature, illuminance) caused by direct sunlight.</rdfs:comment>
    </owl:Class>
    


    <!-- http://w3id.org/eepsa#PostalAddress -->

    <owl:Class rdf:about="http://w3id.org/eepsa#PostalAddress">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://w3id.org/eepsa#postalAddress_hasAddress"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://w3id.org/eepsa#postalAddress_hasCountry"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://w3id.org/eepsa#postalAddress_hasPostalCode"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://w3id.org/eepsa#postalAddress_hasRegion"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://w3id.org/eepsa#postalAddress_hasTown"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">This entity represents an address for delivery of paper based mail and other postal deliveries (IFC4Add2)</rdfs:comment>
        <rdfs:seeAlso rdf:resource="http://ifcowl.openbimstandards.org/IFC4_ADD2#IfcPostalAddress"/>
    </owl:Class>
    


    <!-- http://w3id.org/eepsa#Roof -->

    <owl:Class rdf:about="http://w3id.org/eepsa#Roof">
        <rdfs:subClassOf rdf:resource="http://w3id.org/eepsa#ExternalBuildingElement"/>
        <rdfs:comment xml:lang="en">Definition according to ISO 6707-1: construction enclosing the building from above.</rdfs:comment>
        <rdfs:seeAlso rdf:resource="http://ifcowl.openbimstandards.org/IFC4_ADD2#IfcRoof"/>
    </owl:Class>
    


    <!-- http://w3id.org/eepsa#Skylight -->

    <owl:Class rdf:about="http://w3id.org/eepsa#Skylight">
        <rdfs:subClassOf rdf:resource="http://w3id.org/eepsa#ExternalBuildingElement"/>
        <rdfs:comment xml:lang="en">Skylights are light transmitting fenestration (elements filling building envelope openings) forming all, or a portion of, the roof of a building&apos;s space for daylighting purposes. (Wikipedia)</rdfs:comment>
    </owl:Class>
    


    <!-- http://w3id.org/eepsa#SouthOrientation -->

    <owl:Class rdf:about="http://w3id.org/eepsa#SouthOrientation">
        <rdfs:subClassOf rdf:resource="http://w3id.org/eepsa#Orientation"/>
    </owl:Class>
    


    <!-- http://w3id.org/eepsa#SpaceOccupancy -->

    <owl:Class rdf:about="http://w3id.org/eepsa#SpaceOccupancy">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/ssn/Property"/>
    </owl:Class>
    


    <!-- http://w3id.org/eepsa#TemperatureSensor -->

    <owl:Class rdf:about="http://w3id.org/eepsa#TemperatureSensor">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/ssn/SensingDevice"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.w3.org/ns/ssn/observes"/>
                <owl:allValuesFrom rdf:resource="http://purl.org/iot/vocab/m3-lite#Temperature"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://w3id.org/eepsa#Wall -->

    <owl:Class rdf:about="http://w3id.org/eepsa#Wall">
        <rdfs:subClassOf rdf:resource="http://w3id.org/eepsa#BuildingElement"/>
        <owl:disjointWith rdf:resource="http://w3id.org/eepsa#Window"/>
        <rdfs:comment>Definition according to ISO 6707-1: vertical construction usually in masonry or in concrete which bounds or subdivides a construction works and fulfils a load bearing or retaining function.</rdfs:comment>
        <rdfs:seeAlso rdf:resource="http://ifcowl.openbimstandards.org/IFC4_ADD2#IfcWall"/>
    </owl:Class>
    


    <!-- http://w3id.org/eepsa#WeatherForecasting -->

    <owl:Class rdf:about="http://w3id.org/eepsa#WeatherForecasting">
        <rdfs:subClassOf rdf:resource="http://w3id.org/eepsa#WeatherService"/>
    </owl:Class>
    


    <!-- http://w3id.org/eepsa#WeatherReport -->

    <owl:Class rdf:about="http://w3id.org/eepsa#WeatherReport">
        <rdfs:subClassOf rdf:resource="http://w3id.org/eepsa#WeatherService"/>
    </owl:Class>
    


    <!-- http://w3id.org/eepsa#WeatherService -->

    <owl:Class rdf:about="http://w3id.org/eepsa#WeatherService"/>
    


