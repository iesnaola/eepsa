    <rdf:Description rdf:about="http://www.w3.org/2003/01/geo/wgs84_pos#lat_long">
        <rdfs:comment>A comma-separated representation of a latitude, longitude coordinate.</rdfs:comment>
        <rdfs:label>lat/long</rdfs:label>
