     -->

    


    <!-- http://purl.oclc.org/NET/ssnx/ssn#implements -->

    <rdf:Description rdf:about="http://purl.oclc.org/NET/ssnx/ssn#implements">
        <owl:equivalentProperty rdf:resource="http://www.w3.org/ns/ssn/implements"/>
    </rdf:Description>
    


    <!-- http://w3id.org/eepsa#capturedIn -->

    <owl:ObjectProperty rdf:about="http://w3id.org/eepsa#capturedIn">
        <owl:inverseOf rdf:resource="http://w3id.org/eepsa#captures"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="http://w3id.org/eepsa#containsPhysicalObject"/>
            <rdf:Description>
                <owl:inverseOf rdf:resource="http://www.w3.org/ns/ssn/observedBy"/>
            </rdf:Description>
        </owl:propertyChainAxiom>
        <rdfs:comment xml:lang="en">Relation between an Observation and the Spatial Thing (Building, Space) where it has been captured</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://w3id.org/eepsa#captures -->

    <owl:ObjectProperty rdf:about="http://w3id.org/eepsa#captures">
        <rdfs:comment xml:lang="en">Relation between a Spatial Thing (Building, Space) and the Observation that has been captured</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://w3id.org/eepsa#containedInSpatialStructure -->

    <owl:ObjectProperty rdf:about="http://w3id.org/eepsa#containedInSpatialStructure">
        <owl:inverseOf rdf:resource="http://w3id.org/eepsa#containsBuildingElement"/>
        <rdfs:comment xml:lang="en">A relation between a Building Element and the Spatial Structure (Space or Building) in which it is contained</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://ifcowl.openbimstandards.org/IFC4_ADD2#IfcRelContainedInSpatialStructure"/>
    </owl:ObjectProperty>
    


    <!-- http://w3id.org/eepsa#containsBuildingElement -->

    <owl:ObjectProperty rdf:about="http://w3id.org/eepsa#containsBuildingElement">
        <rdfs:comment>Relation used to assign Building Element to a certain Spatial Structure (Space or Building).</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://w3id.org/eepsa#containsPhysicalObject -->

    <owl:ObjectProperty rdf:about="http://w3id.org/eepsa#containsPhysicalObject">
        <owl:inverseOf rdf:resource="http://w3id.org/eepsa#isContainedInPhysicalSpace"/>
        <rdfs:comment>A relation between a physical space and the objects located in such space.</rdfs:comment>
        <rdfs:seeAlso rdf:resource="https://w3id.org/def/saref4bldg#contains"/>
    </owl:ObjectProperty>
    


    <!-- http://w3id.org/eepsa#hasDataSource -->

    <owl:ObjectProperty rdf:about="http://w3id.org/eepsa#hasDataSource">
        <rdfs:comment>Relation between a Quantity Kind and a data source where values can be retrieved</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://w3id.org/eepsa#hasOrientation -->

    <owl:ObjectProperty rdf:about="http://w3id.org/eepsa#hasOrientation">
        <rdfs:comment>Relation between a thing and the direction it is oriented</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://w3id.org/eepsa#hasPostalAddress -->

    <owl:ObjectProperty rdf:about="http://w3id.org/eepsa#hasPostalAddress">
        <rdfs:range rdf:resource="http://w3id.org/eepsa#PostalAddress"/>
        <rdfs:comment xml:lang="en">Relation between a Building and its Postal Address</rdfs:comment>
        <rdfs:seeAlso rdf:resource="http://ifcowl.openbimstandards.org/IFC4_ADD2#buildingAddress_IfcBuilding"/>
    </owl:ObjectProperty>
    


    <!-- http://w3id.org/eepsa#hasSolvingMethod -->

    <owl:ObjectProperty rdf:about="http://w3id.org/eepsa#hasSolvingMethod">
        <rdfs:comment>A relation between a Thing (mainly an Outlier) and a Method proposed to solve it</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://w3id.org/eepsa#hasSpace -->

    <owl:ObjectProperty rdf:about="http://w3id.org/eepsa#hasSpace">
        <owl:inverseOf rdf:resource="http://w3id.org/eepsa#isSpaceOf"/>
        <rdfs:comment>Relation between a building or a building space and the spaces it can be divided into.</rdfs:comment>
        <rdfs:seeAlso rdf:resource="https://w3id.org/def/saref4bldg#hasSpace"/>
    </owl:ObjectProperty>
    


    <!-- http://w3id.org/eepsa#hasUnitOfMeasure -->

    <owl:ObjectProperty rdf:about="http://w3id.org/eepsa#hasUnitOfMeasure">
        <rdfs:comment xml:lang="en">Relation between a Thing (Sensor) and a Unit of measurement</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://w3id.org/eepsa#isAffectedBy -->

    <owl:ObjectProperty rdf:about="http://w3id.org/eepsa#isAffectedBy">
        <rdfs:range rdf:resource="http://qudt.org/schema/qudt#QuantityKind"/>
        <rdfs:comment>Relationship between a space and the Quantity Kind that is affects its indoor conditions</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://w3id.org/eepsa#isContainedInPhysicalSpace -->

    <owl:ObjectProperty rdf:about="http://w3id.org/eepsa#isContainedInPhysicalSpace">
        <rdfs:comment>A relation between an object and the physical space in which it is located.</rdfs:comment>
        <rdfs:seeAlso rdf:resource="https://w3id.org/def/saref4bldg#isContainedIn"/>
    </owl:ObjectProperty>
    


    <!-- http://w3id.org/eepsa#isHighlyAffectedBy -->

    <owl:ObjectProperty rdf:about="http://w3id.org/eepsa#isHighlyAffectedBy">
        <rdfs:subPropertyOf rdf:resource="http://w3id.org/eepsa#isAffectedBy"/>
        <rdfs:comment>Relationship between a space and the Quantity Kind that is affects its indoor conditions with a higher degree</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://w3id.org/eepsa#isSpaceOf -->

    <owl:ObjectProperty rdf:about="http://w3id.org/eepsa#isSpaceOf">
        <rdfs:comment>Relation between a building space and the building or building space it belongs to.</rdfs:comment>
        <rdfs:seeAlso rdf:resource="https://w3id.org/def/saref4bldg#isSpaceOf"/>
    </owl:ObjectProperty>
    


    <!-- http://w3id.org/eepsa#madeFor -->

    <owl:ObjectProperty rdf:about="http://w3id.org/eepsa#madeFor">
        <rdfs:comment>Relation between a Weather Service and the location for which it has been made 

*** REVISAR EL NOMBRE
Relaciona weather services (reports o forecasts) realizados en o para un lugar (weather station) concreto. No tiene por qué ser weather station, hay veces que los forecasts se realizan para una región</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://w3id.org/eepsa#providesWeatherService -->

    <owl:ObjectProperty rdf:about="http://w3id.org/eepsa#providesWeatherService">
        <rdfs:domain rdf:resource="http://w3id.org/eepsa#WeatherStation"/>
        <rdfs:comment>Relation between a Thing (weather station) and the Weather Service it provices (weather report, weather forecast,...)</rdfs:comment>
    </owl:ObjectProperty>
    


