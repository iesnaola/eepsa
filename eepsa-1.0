    <!-- http://purl.oclc.org/NET/ssnx/ssn#Device -->

    <rdf:Description rdf:about="http://purl.oclc.org/NET/ssnx/ssn#Device">
        <owl:equivalentClass rdf:resource="http://www.w3.org/ns/ssn/Device"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://w3id.org/eepsa#shelteredDevice"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onDataRange rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
