    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/eepsa#manufacturedBy -->

    <owl:DatatypeProperty rdf:about="https://w3id.org/eepsa#manufacturedBy">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en">Determines the manufacturer of a Thing</rdfs:comment>
        <rdfs:label xml:lang="en">manufactured by</rdfs:label>
        <skos:example xml:lang="en">Example:
myOnto:Vehicle1 eepsa:manufacturedBy xsd:string(&quot;Volkswagen&quot;).</skos:example>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/eepsa#model -->

    <owl:DatatypeProperty rdf:about="https://w3id.org/eepsa#model">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Model of a Thing (normally a Physical Object)</rdfs:comment>
        <rdfs:label xml:lang="en">model</rdfs:label>
        <skos:example xml:lang="en">Example:
myOnto:Vehicle1 eepsa:model xsd:string(&quot;Golf GTI&quot;).</skos:example>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/eepsa#obsDate -->

    <owl:DatatypeProperty rdf:about="https://w3id.org/eepsa#obsDate">
        <rdfs:comment>Determines the Date (e.g. yyyy-MM-dd) of an observation.</rdfs:comment>
        <rdfs:label xml:lang="en">observation date</rdfs:label>
        <skos:example xml:lang="en">Example:
myOnto:obs1 eepsa:observationDate &quot;2017-02-15&quot;.</skos:example>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/eepsa#obsTime -->

    <owl:DatatypeProperty rdf:about="https://w3id.org/eepsa#obsTime">
        <rdfs:comment>Determines the Time (e.g. hh:mm:ss) of an observation.</rdfs:comment>
        <rdfs:label xml:lang="en">observation time</rdfs:label>
        <skos:example xml:lang="en">Example:
myOnto:obs1 eepsa:observationTime &quot;12:00:00&quot;.</skos:example>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/eepsa#startingTime -->

    <owl:DatatypeProperty rdf:about="https://w3id.org/eepsa#startingTime">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#time"/>
        <rdfs:comment xml:lang="en">Determines the Starting time of a temporal entity.</rdfs:comment>
        <rdfs:label xml:lang="en">starting time</rdfs:label>
    </owl:DatatypeProperty>
    


