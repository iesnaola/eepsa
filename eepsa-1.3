    <!-- http://qudt.org/schema/qudt#unit -->

    <owl:ObjectProperty rdf:about="http://qudt.org/schema/qudt#unit"/>
    


    <!-- http://www.w3.org/2003/01/geo/wgs84_pos#location -->

    <owl:ObjectProperty rdf:about="http://www.w3.org/2003/01/geo/wgs84_pos#location"/>
    


    <!-- https://w3id.org/eepsa#capturedIn -->

    <owl:ObjectProperty rdf:about="https://w3id.org/eepsa#capturedIn">
        <owl:inverseOf rdf:resource="https://w3id.org/eepsa#captures"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="https://w3id.org/bot#containsElement"/>
            <rdf:Description>
                <owl:inverseOf rdf:resource="http://www.w3.org/ns/sosa/isObservedBy"/>
            </rdf:Description>
        </owl:propertyChainAxiom>
        <rdfs:comment xml:lang="en">Relation between an Observation and the Spatial Thing (Building, Space) where it has been captured</rdfs:comment>
        <rdfs:label xml:lang="en">captured in</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/eepsa#captures -->

    <owl:ObjectProperty rdf:about="https://w3id.org/eepsa#captures">
        <rdfs:comment xml:lang="en">Relation between a Spatial Thing (Building, Space) and the Observation that has been captured</rdfs:comment>
        <rdfs:label xml:lang="en">captures</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/eepsa#hasDataSource -->

    <owl:ObjectProperty rdf:about="https://w3id.org/eepsa#hasDataSource">
        <rdfs:comment xml:lang="en">Relation between a Thing and a data source where values can be retrieved.</rdfs:comment>
        <rdfs:label xml:lang="en">has data source</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/eepsa#hasSolvingMethod -->

    <owl:ObjectProperty rdf:about="https://w3id.org/eepsa#hasSolvingMethod">
        <rdfs:range rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Method"/>
        <rdfs:comment>A relation between a Thing (mainly an Outlier) and a Method proposed to solve it</rdfs:comment>
        <rdfs:label xml:lang="en">has solving method</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/eepsa#hasSunExposurePeriod -->

    <owl:ObjectProperty rdf:about="https://w3id.org/eepsa#hasSunExposurePeriod">
        <rdfs:comment>Determines the period to which it is exposed to sun</rdfs:comment>
        <rdfs:label xml:lang="en">has sun exposure period</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/eepsa#isAffectedBy -->

    <owl:ObjectProperty rdf:about="https://w3id.org/eepsa#isAffectedBy">
        <rdfs:domain rdf:resource="https://w3id.org/bot#Space"/>
        <rdfs:range rdf:resource="http://www.w3.org/ns/ssn/Property"/>
        <rdfs:comment xml:lang="en">Relationship between a space and the Quantity Kind that is affects its indoor conditions</rdfs:comment>
        <rdfs:label xml:lang="en">is affected by</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/eepsa#isForecastedToProduceSituation -->

    <owl:ObjectProperty rdf:about="https://w3id.org/eepsa#isForecastedToProduceSituation">
        <rdfs:range rdf:resource="https://w3id.org/eepsa#Situation"/>
        <rdfs:comment xml:lang="en">A property that links a Thing, with a Situation that is forecasted to produce</rdfs:comment>
        <rdfs:label xml:lang="en">is forecasted to produce situation</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/eepsa#isHighlyAffectedBy -->

    <owl:ObjectProperty rdf:about="https://w3id.org/eepsa#isHighlyAffectedBy">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/eepsa#isAffectedBy"/>
        <rdfs:range rdf:resource="http://www.w3.org/ns/ssn/Property"/>
        <rdfs:comment xml:lang="en">Relationship between a space and the Quantity Kind that is affects its indoor conditions with a higher degree</rdfs:comment>
        <rdfs:label xml:lang="en">is highly affected by</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/eepsa#providesWeatherService -->

    <owl:ObjectProperty rdf:about="https://w3id.org/eepsa#providesWeatherService">
        <rdfs:range rdf:resource="https://w3id.org/eepsa#WeatherService"/>
        <rdfs:comment xml:lang="en">Relation between a Weather Station and the Weather Service it provides (weather report, weather forecast,...)</rdfs:comment>
        <rdfs:label xml:lang="en">provides weather service</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/eepsa#susceptibleToOutliersCausedBy -->

    <owl:ObjectProperty rdf:about="https://w3id.org/eepsa#susceptibleToOutliersCausedBy">
        <rdfs:comment xml:lang="en">Determines the cause that makes a sensing devices susceptible to suffer from Outliers.</rdfs:comment>
        <rdfs:label xml:lang="en">susceptible to outliers caused by</rdfs:label>
    </owl:ObjectProperty>
    


