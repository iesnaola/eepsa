@prefix : <https://w3id.org/eepsa#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <https://w3id.org/eepsa> .

<https://w3id.org/eepsa> rdf:type owl:Ontology ;
                          owl:versionIRI <https://raw.githubusercontent.com/iesnaola/eepsa/master/EEPSA_previousVersions/eepsa-1.3.owl> ;
                          owl:imports <http://www.w3.org/2006/time> ,
                                      <https://raw.githubusercontent.com/iesnaola/eepsa/master/EEPSA_previousVersions/datamining4eepsa-0.1.owl> ,
                                      <https://raw.githubusercontent.com/iesnaola/eepsa/master/EEPSA_previousVersions/forecasting4eepsa-0.2.owl> ,
                                      <https://raw.githubusercontent.com/iesnaola/eepsa/master/EEPSA_previousVersions/measurements4eepsa-0.3.owl> ,
                                      <https://w3id.org/bim4eepsa> ;
                          <http://creativecommons.org/ns#license> <https://creativecommons.org/licenses/by-nc/4.0/> ;
                          <http://purl.org/dc/terms/created> "2016-02-20" ;
                          <http://purl.org/dc/terms/creator> """Iker Esnaola-Gonzalez <mailto:iker.esnaola@tekniker.es>
Jesús Bermúdez <mailto:jesus.bermudez@ehu.eus>""" ;
                          <http://purl.org/dc/terms/description> "EEPSA (Energy Efficiency Prediction Semantic Assistant) Ontology aims to capture knowledge related to buildings, sensing and actuating devices, and their corresponding observations and actuations, towards the support of data analysts in energy efficiency problems." ;
                          <http://purl.org/dc/terms/issued> "2017-07-21" ;
                          <http://purl.org/dc/terms/modified> "2018-01-02" ;
                          <http://purl.org/dc/terms/title> "EEPSA (Energy Efficiency Prediction Semantic Assistant) Ontology" ;
                          <http://purl.org/ontology/bibo/status> "Work in Progress"@en ;
                          <http://purl.org/vocab/vann/preferredNamespacePrefix> "eepsa" ;
                          <http://purl.org/vocab/vann/preferredNamespaceUri> : ;
                          rdfs:comment """Information about changes compared to version 1.2
*********************************************************
•	The classes and properties related to the building modelling (imported from bim4EEPSA) are now described using the BOT Ontology.
•	The f4EEPSA (Forecasting for EEPSA) module is now imported.""" ,
                                       """The ontology is documented in:

Iker Esnaola-Gonzalez, Jesús Bermúdez, Izaskun Fernandez, Aitor Arnaiz, (2018) Semantic Prediction Assistant Approach applied to Energy Efficiency in Tertiary Buildings http://www.semantic-web-journal.net/content/semantic-prediction-assistant-approach-applied-energy-efficiency-tertiary-buildings-1""" ;
                          owl:versionInfo 1.3 .

#################################################################
#    Annotation properties
#################################################################

###  http://creativecommons.org/ns#license
<http://creativecommons.org/ns#license> rdf:type owl:AnnotationProperty .


###  http://purl.oclc.org/NET/UNIS/iot-lite/iot-lite#relativeLocation
<http://purl.oclc.org/NET/UNIS/iot-lite/iot-lite#relativeLocation> rdf:type owl:AnnotationProperty ;
                                                                   rdfs:comment "A location not provided by geo-coordinates (i.e. building A, city of Aarhus)."@en ;
                                                                   rdfs:isDefinedBy <http://purl.oclc.org/NET/UNIS/fiware/iot-lite#> ;
                                                                   rdfs:domain <http://www.w3.org/2003/01/geo/wgs84_pos#Point> .


###  http://purl.org/dc/elements/1.1/date
<http://purl.org/dc/elements/1.1/date> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/description
<http://purl.org/dc/elements/1.1/description> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/title
<http://purl.org/dc/elements/1.1/title> rdf:type owl:AnnotationProperty .


###  http://purl.org/ontology/bibo/status
<http://purl.org/ontology/bibo/status> rdf:type owl:AnnotationProperty .


###  http://purl.org/vocab/vann/preferredNamespacePrefix
<http://purl.org/vocab/vann/preferredNamespacePrefix> rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2002/07/owl#minQualifiedCardinality
owl:minQualifiedCardinality rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2003/01/geo/wgs84_pos#alt
<http://www.w3.org/2003/01/geo/wgs84_pos#alt> rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2003/01/geo/wgs84_pos#lat
<http://www.w3.org/2003/01/geo/wgs84_pos#lat> rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2003/01/geo/wgs84_pos#location
<http://www.w3.org/2003/01/geo/wgs84_pos#location> rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2003/01/geo/wgs84_pos#long
<http://www.w3.org/2003/01/geo/wgs84_pos#long> rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2004/02/skos/core#example
<http://www.w3.org/2004/02/skos/core#example> rdf:type owl:AnnotationProperty .


###  http://xmlns.com/foaf/0.1/based_near
<http://xmlns.com/foaf/0.1/based_near> rdf:type owl:AnnotationProperty .


###  http://xmlns.com/foaf/0.1/depiction
<http://xmlns.com/foaf/0.1/depiction> rdf:type owl:AnnotationProperty .


###  http://xmlns.com/foaf/0.1/name
<http://xmlns.com/foaf/0.1/name> rdfs:range rdfs:Literal .


#################################################################
#    Datatypes
#################################################################

###  http://www.w3.org/2001/XMLSchema#time
xsd:time rdf:type rdfs:Datatype .


#################################################################
#    Object Properties
#################################################################

###  https://w3id.org/bot#containsElement
<https://w3id.org/bot#containsElement> rdf:type owl:ObjectProperty .


###  https://w3id.org/eepsa#capturedIn
:capturedIn rdf:type owl:ObjectProperty ;
            owl:inverseOf :captures ;
            owl:propertyChainAxiom ( <https://w3id.org/bot#containsElement>
                                     [ owl:inverseOf <http://www.w3.org/ns/sosa/isObservedBy>
                                     ]
                                   ) ;
            rdfs:comment "Relation between an Observation and the Spatial Thing (Building, Space) where it has been captured"@en ;
            rdfs:label "captured in"@en .


###  https://w3id.org/eepsa#captures
:captures rdf:type owl:ObjectProperty ;
          rdfs:comment "Relation between a Spatial Thing (Building, Space) and the Observation that has been captured"@en ;
          rdfs:label "captures"@en .


###  https://w3id.org/eepsa#hasDataSource
:hasDataSource rdf:type owl:ObjectProperty ;
               rdfs:comment "Relation between a Thing and a data source where values can be retrieved."@en ;
               rdfs:label "has data source"@en .


###  https://w3id.org/eepsa#hasSolvingMethod
:hasSolvingMethod rdf:type owl:ObjectProperty ;
                  rdfs:range <http://www.w3.org/ns/sosa/Procedure> ;
                  rdfs:comment "A relation between a Thing (mainly an Outlier) and a Procedure proposed to solve it."@en ;
                  rdfs:label "has solving method"@en .


###  https://w3id.org/eepsa#hasSunExposurePeriod
:hasSunExposurePeriod rdf:type owl:ObjectProperty ;
                      rdfs:comment "Determines the period to which it is exposed to sun" ;
                      rdfs:label "has sun exposure period"@en .


###  https://w3id.org/eepsa#isAffectedBy
:isAffectedBy rdf:type owl:ObjectProperty ;
              rdfs:domain <https://w3id.org/bot#Space> ;
              rdfs:range <http://www.w3.org/ns/ssn/Property> ;
              rdfs:comment "Relationship between a space and the Quantity Kind that is affects its indoor conditions"@en ;
              rdfs:label "is affected by"@en .


###  https://w3id.org/eepsa#isForecastedToProduceSituation
:isForecastedToProduceSituation rdf:type owl:ObjectProperty ;
                                rdfs:comment "A property that links a Thing, with a Situation that is forecasted to produce"@en ;
                                rdfs:label "is forecasted to produce situation"@en .


###  https://w3id.org/eepsa#isHighlyAffectedBy
:isHighlyAffectedBy rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf :isAffectedBy ;
                    rdfs:range <http://www.w3.org/ns/ssn/Property> ;
                    rdfs:comment "Relationship between a space and the Quantity Kind that is affects its indoor conditions with a higher degree"@en ;
                    rdfs:label "is highly affected by"@en .


###  https://w3id.org/eepsa#providesWeatherService
:providesWeatherService rdf:type owl:ObjectProperty ;
                        rdfs:range :WeatherService ;
                        rdfs:comment "Relation between a Weather Station and the Weather Service it provides (weather report, weather forecast,...)"@en ;
                        rdfs:label "provides weather service"@en .


#################################################################
#    Data properties
#################################################################

###  http://qudt.org/schema/qudt#numericValue
<http://qudt.org/schema/qudt#numericValue> rdf:type owl:DatatypeProperty ;
                                           rdfs:range xsd:double .


###  https://w3id.org/eepsa#endingTime
:endingTime rdf:type owl:DatatypeProperty ;
            rdfs:range xsd:time ;
            rdfs:comment "Determines the Ending Time of a temporal entity."@en ;
            rdfs:label "ending time"@en .


###  https://w3id.org/eepsa#hasMonth
:hasMonth rdf:type owl:DatatypeProperty ;
          rdfs:label "has month"@en .


###  https://w3id.org/eepsa#startingTime
:startingTime rdf:type owl:DatatypeProperty ;
              rdfs:range xsd:time ;
              rdfs:comment "Determines the Starting time of a temporal entity."@en ;
              rdfs:label "starting time"@en .


#################################################################
#    Classes
#################################################################

###  http://purl.org/iot/vocab/m3-lite#AirPollution
<http://purl.org/iot/vocab/m3-lite#AirPollution> rdf:type owl:Class ;
                                                 rdfs:subClassOf :MeteorologicalProperty .


###  http://purl.org/iot/vocab/m3-lite#AirQuality
<http://purl.org/iot/vocab/m3-lite#AirQuality> rdf:type owl:Class ;
                                               rdfs:subClassOf :MeteorologicalProperty .


###  http://purl.org/iot/vocab/m3-lite#AirTemperature
<http://purl.org/iot/vocab/m3-lite#AirTemperature> rdf:type owl:Class ;
                                                   rdfs:subClassOf :MeteorologicalProperty .


###  http://purl.org/iot/vocab/m3-lite#CO2
<http://purl.org/iot/vocab/m3-lite#CO2> rdf:type owl:Class ;
                                        rdfs:subClassOf :MeteorologicalProperty .


###  http://purl.org/iot/vocab/m3-lite#ChemicalAgentAtmosphericConcentration
<http://purl.org/iot/vocab/m3-lite#ChemicalAgentAtmosphericConcentration> rdf:type owl:Class ;
                                                                          rdfs:subClassOf :MeteorologicalProperty .


###  http://purl.org/iot/vocab/m3-lite#CloudCover
<http://purl.org/iot/vocab/m3-lite#CloudCover> rdf:type owl:Class ;
                                               rdfs:subClassOf :MeteorologicalProperty .


###  http://purl.org/iot/vocab/m3-lite#Conductivity
<http://purl.org/iot/vocab/m3-lite#Conductivity> rdf:type owl:Class .


###  http://purl.org/iot/vocab/m3-lite#DewPoint
<http://purl.org/iot/vocab/m3-lite#DewPoint> rdf:type owl:Class ;
                                             rdfs:subClassOf :MeteorologicalProperty .


###  http://purl.org/iot/vocab/m3-lite#Illuminance
<http://purl.org/iot/vocab/m3-lite#Illuminance> rdf:type owl:Class ;
                                                rdfs:subClassOf :MeteorologicalProperty .


###  http://purl.org/iot/vocab/m3-lite#LeafWetness
<http://purl.org/iot/vocab/m3-lite#LeafWetness> rdf:type owl:Class ;
                                                rdfs:subClassOf :MeteorologicalProperty .


###  http://purl.org/iot/vocab/m3-lite#Precipitation
<http://purl.org/iot/vocab/m3-lite#Precipitation> rdf:type owl:Class ;
                                                  rdfs:subClassOf :MeteorologicalProperty .


###  http://purl.org/iot/vocab/m3-lite#Rainfall
<http://purl.org/iot/vocab/m3-lite#Rainfall> rdf:type owl:Class ;
                                             rdfs:subClassOf :MeteorologicalProperty .


###  http://purl.org/iot/vocab/m3-lite#SolarRadiation
<http://purl.org/iot/vocab/m3-lite#SolarRadiation> rdf:type owl:Class ;
                                                   rdfs:subClassOf :MeteorologicalProperty .


###  http://purl.org/iot/vocab/m3-lite#SunPositionDirection
<http://purl.org/iot/vocab/m3-lite#SunPositionDirection> rdf:type owl:Class ;
                                                         rdfs:subClassOf :MeteorologicalProperty .


###  http://purl.org/iot/vocab/m3-lite#SunPositionElevation
<http://purl.org/iot/vocab/m3-lite#SunPositionElevation> rdf:type owl:Class ;
                                                         rdfs:subClassOf :MeteorologicalProperty .


###  http://purl.org/iot/vocab/m3-lite#WindChill
<http://purl.org/iot/vocab/m3-lite#WindChill> rdf:type owl:Class ;
                                              rdfs:subClassOf :MeteorologicalProperty .


###  http://purl.org/iot/vocab/m3-lite#WindDirection
<http://purl.org/iot/vocab/m3-lite#WindDirection> rdf:type owl:Class ;
                                                  rdfs:subClassOf :MeteorologicalProperty .


###  http://purl.org/iot/vocab/m3-lite#WindSpeed
<http://purl.org/iot/vocab/m3-lite#WindSpeed> rdf:type owl:Class ;
                                              rdfs:subClassOf :MeteorologicalProperty .


###  https://w3id.org/bim4eepsa#WeatherStation
<https://w3id.org/bim4eepsa#WeatherStation> rdfs:subClassOf [ rdf:type owl:Restriction ;
                                                              owl:onProperty :providesWeatherService ;
                                                              owl:someValuesFrom :WeatherService
                                                            ] .


###  https://w3id.org/bot#Space
<https://w3id.org/bot#Space> rdf:type owl:Class ;
                             rdfs:subClassOf [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                                      owl:onProperty :isAffectedBy ;
                                                                      owl:hasValue :SpaceOccupancy
                                                                    ]
                                                                    [ rdf:type owl:Restriction ;
                                                                      owl:onProperty :isAffectedBy ;
                                                                      owl:hasValue <https://w3id.org/measurements4eepsa#IndoorRelativeHumidity>
                                                                    ]
                                                                    [ rdf:type owl:Restriction ;
                                                                      owl:onProperty :isAffectedBy ;
                                                                      owl:hasValue <https://w3id.org/measurements4eepsa#IndoorTemperature>
                                                                    ]
                                                                  ) ;
                                               rdf:type owl:Class
                                             ] .


###  https://w3id.org/eepsa#AdjacentToOutdoorSpace
:AdjacentToOutdoorSpace rdf:type owl:Class ;
                        owl:equivalentClass [ owl:intersectionOf ( <https://w3id.org/bot#Space>
                                                                   [ rdf:type owl:Restriction ;
                                                                     owl:onProperty <https://w3id.org/bot#containsElement> ;
                                                                     owl:someValuesFrom <https://w3id.org/bim4eepsa#ExternalBuildingElement>
                                                                   ]
                                                                 ) ;
                                              rdf:type owl:Class
                                            ] ;
                        rdfs:subClassOf <https://w3id.org/bot#Space> ,
                                        [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                                 owl:onProperty :isAffectedBy ;
                                                                 owl:hasValue <http://purl.org/iot/vocab/m3-lite#CloudCover>
                                                               ]
                                                               [ rdf:type owl:Restriction ;
                                                                 owl:onProperty :isAffectedBy ;
                                                                 owl:hasValue <http://purl.org/iot/vocab/m3-lite#SolarRadiation>
                                                               ]
                                                               [ rdf:type owl:Restriction ;
                                                                 owl:onProperty :isAffectedBy ;
                                                                 owl:hasValue <http://purl.org/iot/vocab/m3-lite#SunPositionDirection>
                                                               ]
                                                               [ rdf:type owl:Restriction ;
                                                                 owl:onProperty :isAffectedBy ;
                                                                 owl:hasValue <http://purl.org/iot/vocab/m3-lite#SunPositionElevation>
                                                               ]
                                                               [ rdf:type owl:Restriction ;
                                                                 owl:onProperty :isAffectedBy ;
                                                                 owl:hasValue <http://purl.org/iot/vocab/m3-lite#WindSpeed>
                                                               ]
                                                               [ rdf:type owl:Restriction ;
                                                                 owl:onProperty :isAffectedBy ;
                                                                 owl:hasValue <https://w3id.org/measurements4eepsa#OutdoorRelativeHumidity>
                                                               ]
                                                               [ rdf:type owl:Restriction ;
                                                                 owl:onProperty :isAffectedBy ;
                                                                 owl:hasValue <https://w3id.org/measurements4eepsa#OutdoorTemperature>
                                                               ]
                                                             ) ;
                                          rdf:type owl:Class
                                        ] ;
                        rdfs:comment "A space in contact with the exterior by means of at least one building element."@en ;
                        rdfs:label "Adjacent to Outdoor Space"@en .


###  https://w3id.org/eepsa#BadInsulatedSpace
:BadInsulatedSpace rdf:type owl:Class ;
                   rdfs:subClassOf <https://w3id.org/bot#Space> ,
                                   [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                            owl:onProperty :isHighlyAffectedBy ;
                                                            owl:hasValue <https://w3id.org/measurements4eepsa#OutdoorRelativeHumidity>
                                                          ]
                                                          [ rdf:type owl:Restriction ;
                                                            owl:onProperty :isHighlyAffectedBy ;
                                                            owl:hasValue <https://w3id.org/measurements4eepsa#OutdoorTemperature>
                                                          ]
                                                        ) ;
                                     rdf:type owl:Class
                                   ] ;
                   rdfs:comment "A space with bad insulation."@en ;
                   rdfs:label "Bad Insulated Space"@en .


###  https://w3id.org/eepsa#BelowGroundLevelSpace
:BelowGroundLevelSpace rdf:type owl:Class ;
                       rdfs:subClassOf <https://w3id.org/bot#Space> ;
                       rdfs:comment "A space located in a storey below ground."@en ;
                       rdfs:label "Below Ground Level Space"@en .


###  https://w3id.org/eepsa#ComfortableThermalSituation
:ComfortableThermalSituation rdf:type owl:Class ;
                             rdfs:comment "A set of circumstances that ensures a comfortable thermal situation. Themal comfort is understood as that condition of mind that express satisfaction with the thermal environment and is assessed by subjective evaluation. (Source: ANSI/ASHRAE Standard 55-2017)"@en ;
                             rdfs:label "Comfortable Thermal Situation"@en .


###  https://w3id.org/eepsa#EastOrientedObject
:EastOrientedObject rdf:type owl:Class ;
                    owl:equivalentClass [ owl:intersectionOf ( <https://w3id.org/bim4eepsa#PhysicalObject>
                                                               [ rdf:type owl:Restriction ;
                                                                 owl:onProperty <http://purl.org/iot/vocab/m3-lite#hasDirection> ;
                                                                 owl:hasValue <https://w3id.org/measurements4eepsa#eastOrientation>
                                                               ]
                                                             ) ;
                                          rdf:type owl:Class
                                        ] ;
                    rdfs:subClassOf <https://w3id.org/bim4eepsa#PhysicalObject> ;
                    rdfs:comment "Object oriented towards the East"@en ;
                    rdfs:label "East Oriented Object"@en .


###  https://w3id.org/eepsa#HSESituation
:HSESituation rdf:type owl:Class ;
              rdfs:subClassOf :ThermalComfortRegulation ;
              rdfs:comment "A situation that meets themal requirements defined by UK's HSE (Health and Safety Execution) guidelines."@en ,
                           "Temperatures over 16ºC during working hours."@en ;
              rdfs:label "HSE Situation"@en .


###  https://w3id.org/eepsa#INSHTForLightSituation
:INSHTForLightSituation rdf:type owl:Class ;
                        rdfs:subClassOf :ThermalComfortRegulation ;
                        rdfs:comment "A situation that meets themal requirements defined by Spanish INSHT (Instituto Nacional de Seguridad e Higiene en el Trabajo) guidelines for light workplaces."@en ,
                                     "Temperatures between 14ºC and 25ºC during working hours."@en ;
                        rdfs:label "INSHT for Light Situation"@en .


###  https://w3id.org/eepsa#INSHTForSedentarySituation
:INSHTForSedentarySituation rdf:type owl:Class ;
                            rdfs:subClassOf :ThermalComfortRegulation ;
                            rdfs:comment "A situation that meets themal requirements defined by Spanish INSHT (Instituto Nacional de Seguridad e Higiene en el Trabajo) guidelines for sedentary workplaces."@en ,
                                         "Temperatures between 17ºC and 27ºC during working hours."@en ;
                            rdfs:label "INSHT for Sedentary Situation"@en .


###  https://w3id.org/eepsa#MeteorologicalProperty
:MeteorologicalProperty rdf:type owl:Class ;
                        rdfs:subClassOf <http://www.w3.org/ns/sosa/ObservableProperty> ,
                                        [ rdf:type owl:Restriction ;
                                          owl:onProperty :hasDataSource ;
                                          owl:someValuesFrom :WeatherService
                                        ] ;
                        rdfs:comment "Properties that can be measured by weather stations."@en ;
                        rdfs:label "Meteorological Property"@en .


###  https://w3id.org/eepsa#NaturallyEnlightenedSpace
:NaturallyEnlightenedSpace rdf:type owl:Class ;
                           owl:equivalentClass [ owl:intersectionOf ( <https://w3id.org/bot#Space>
                                                                      [ rdf:type owl:Class ;
                                                                        owl:unionOf ( [ rdf:type owl:Restriction ;
                                                                                        owl:onProperty <https://w3id.org/bot#containsElement> ;
                                                                                        owl:someValuesFrom <https://w3id.org/bim4eepsa#ExternalWindow>
                                                                                      ]
                                                                                      [ rdf:type owl:Restriction ;
                                                                                        owl:onProperty <https://w3id.org/bot#containsElement> ;
                                                                                        owl:someValuesFrom <https://w3id.org/bim4eepsa#Skylight>
                                                                                      ]
                                                                                    )
                                                                      ]
                                                                    ) ;
                                                 rdf:type owl:Class
                                               ] ;
                           rdfs:subClassOf <https://w3id.org/bot#Space> ,
                                           [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                                    owl:onProperty :isHighlyAffectedBy ;
                                                                    owl:hasValue <http://purl.org/iot/vocab/m3-lite#CloudCover>
                                                                  ]
                                                                  [ rdf:type owl:Restriction ;
                                                                    owl:onProperty :isHighlyAffectedBy ;
                                                                    owl:hasValue <http://purl.org/iot/vocab/m3-lite#SolarRadiation>
                                                                  ]
                                                                  [ rdf:type owl:Restriction ;
                                                                    owl:onProperty :isHighlyAffectedBy ;
                                                                    owl:hasValue <http://purl.org/iot/vocab/m3-lite#SunPositionDirection>
                                                                  ]
                                                                  [ rdf:type owl:Restriction ;
                                                                    owl:onProperty :isHighlyAffectedBy ;
                                                                    owl:hasValue <http://purl.org/iot/vocab/m3-lite#SunPositionElevation>
                                                                  ]
                                                                ) ;
                                             rdf:type owl:Class
                                           ] ;
                           rdfs:comment "A space enlightened with a source of light from the exterior (and not enlightened by a source of light inside the space)."@en ;
                           rdfs:label "Naturally Enlightened Space"@en .


###  https://w3id.org/eepsa#NorthEastOrientedObject
:NorthEastOrientedObject rdf:type owl:Class ;
                         owl:equivalentClass [ owl:intersectionOf ( <https://w3id.org/bim4eepsa#PhysicalObject>
                                                                    [ rdf:type owl:Restriction ;
                                                                      owl:onProperty <http://purl.org/iot/vocab/m3-lite#hasDirection> ;
                                                                      owl:hasValue <https://w3id.org/measurements4eepsa#northEastOrientation>
                                                                    ]
                                                                  ) ;
                                               rdf:type owl:Class
                                             ] ;
                         rdfs:subClassOf <https://w3id.org/bim4eepsa#PhysicalObject> ;
                         rdfs:comment "Object oriented towards the North East"@en ;
                         rdfs:label "North East Oriented Object"@en .


###  https://w3id.org/eepsa#NorthOrientedObject
:NorthOrientedObject rdf:type owl:Class ;
                     owl:equivalentClass [ owl:intersectionOf ( <https://w3id.org/bim4eepsa#PhysicalObject>
                                                                [ rdf:type owl:Restriction ;
                                                                  owl:onProperty <http://purl.org/iot/vocab/m3-lite#hasDirection> ;
