###  https://w3id.org/pf4eepsa#BreedingSpace
:BreedingSpace rdf:type owl:Class ;
               rdfs:subClassOf <https://w3id.org/bot#Space> ,
                               [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                        owl:onProperty <https://w3id.org/eepsa#isAffectedBy> ;
                                                        owl:hasValue <http://purl.org/iot/vocab/m3-lite#CO2>
                                                      ]
                                                      [ rdf:type owl:Restriction ;
                                                        owl:onProperty <https://w3id.org/eepsa#isAffectedBy> ;
                                                        owl:hasValue :StockingDensity
                                                      ]
                                                    ) ;
                                 rdf:type owl:Class
                               ] ;
               rdfs:comment "A space where animals are raised and fed."@en ;
               rdfs:isDefinedBy : ;
               rdfs:label "Breeding Space"@en ;
               <http://www.w3.org/2003/06/sw-vocab-status/ns#term_status> "stable" .


###  https://w3id.org/pf4eepsa#Farm
:Farm rdf:type owl:Class ;
      rdfs:subClassOf <https://w3id.org/bot#Building> ;
      rdfs:comment "A farm is an area of land that is devoted primarily to agricultural processes with the primary objective of producing food and other crops; it is the basic facility in food production (Source: Wikipedia)."@en ;
      rdfs:isDefinedBy : ;
      rdfs:label "Farm"@en ;
      <http://www.w3.org/2003/06/sw-vocab-status/ns#term_status> "stable" .


