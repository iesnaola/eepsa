                                                    rdfs:subClassOf [ rdf:type owl:Restriction ;
                                                                      owl:onProperty <http://www.w3.org/ns/ssn/observes> ;
                                                                      owl:allValuesFrom <http://purl.org/iot/vocab/m3-lite#WindChill>
                                                                    ] .


###  http://purl.org/iot/vocab/m3-lite#WindDirection
<http://purl.org/iot/vocab/m3-lite#WindDirection> rdf:type owl:Class ;
                                                  rdfs:subClassOf :MeteorologicalProperty .


###  http://purl.org/iot/vocab/m3-lite#WindDirectionSensor
<http://purl.org/iot/vocab/m3-lite#WindDirectionSensor> rdf:type owl:Class ;
                                                        rdfs:subClassOf [ rdf:type owl:Restriction ;
                                                                          owl:onProperty <http://www.w3.org/ns/ssn/observes> ;
                                                                          owl:allValuesFrom <http://purl.org/iot/vocab/m3-lite#WindDirection>
                                                                        ] .


###  http://purl.org/iot/vocab/m3-lite#WindSpeed
<http://purl.org/iot/vocab/m3-lite#WindSpeed> rdf:type owl:Class ;
                                              rdfs:subClassOf :MeteorologicalProperty .


###  http://purl.org/iot/vocab/m3-lite#WindSpeedSensor
<http://purl.org/iot/vocab/m3-lite#WindSpeedSensor> rdf:type owl:Class ;
                                                    rdfs:subClassOf [ rdf:type owl:Restriction ;
                                                                      owl:onProperty <http://www.w3.org/ns/ssn/observes> ;
                                                                      owl:allValuesFrom <http://purl.org/iot/vocab/m3-lite#WindSpeed>
                                                                    ] .


###  http://w3id.org/eepsa#AdjacentToOutdoorSpace
:AdjacentToOutdoorSpace rdf:type owl:Class ;
                        owl:equivalentClass [ rdf:type owl:Class ;
                                              owl:unionOf ( [ owl:intersectionOf ( :BuildingSpace
                                                                                   [ rdf:type owl:Restriction ;
                                                                                     owl:onProperty :containsBuildingElement ;
                                                                                     owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                                                     owl:onClass :ExternalBuildingElement
                                                                                   ]
                                                                                 ) ;
                                                              rdf:type owl:Class
                                                            ]
                                                            [ owl:intersectionOf ( :BuildingSpace
                                                                                   [ rdf:type owl:Restriction ;
                                                                                     owl:onProperty :pset_isExternal ;
                                                                                     owl:hasValue "true"^^xsd:boolean
                                                                                   ]
                                                                                 ) ;
                                                              rdf:type owl:Class
                                                            ]
                                                          )
                                            ] ;
                        rdfs:subClassOf :BuildingSpace ,
                                        [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                                 owl:onProperty :isAffectedBy ;
                                                                 owl:hasValue <http://purl.org/iot/vocab/m3-lite#CloudCover>
                                                               ]
                                                               [ rdf:type owl:Restriction ;
                                                                 owl:onProperty :isAffectedBy ;
                                                                 owl:hasValue <http://purl.org/iot/vocab/m3-lite#SolarRadiation>
                                                               ]
                                                               [ rdf:type owl:Restriction ;
                                                                 owl:onProperty :isAffectedBy ;
                                                                 owl:hasValue <http://purl.org/iot/vocab/m3-lite#SunPositionDirection>
                                                               ]
                                                               [ rdf:type owl:Restriction ;
                                                                 owl:onProperty :isAffectedBy ;
                                                                 owl:hasValue <http://purl.org/iot/vocab/m3-lite#SunPositionElevation>
                                                               ]
                                                             ) ;
                                          rdf:type owl:Class
                                        ] ,
                                        [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                                 owl:onProperty :isAffectedBy ;
                                                                 owl:hasValue :OutdoorRelativeHumidity
                                                               ]
                                                               [ rdf:type owl:Restriction ;
                                                                 owl:onProperty :isAffectedBy ;
                                                                 owl:hasValue :OutdoorTemperature
                                                               ]
                                                             ) ;
                                          rdf:type owl:Class
                                        ] ,
                                        [ rdf:type owl:Restriction ;
                                          owl:onProperty :isAffectedBy ;
                                          owl:hasValue <http://purl.org/iot/vocab/m3-lite#WindSpeed>
                                        ] .


###  http://w3id.org/eepsa#BadInsulatedSpace
:BadInsulatedSpace rdf:type owl:Class ;
                   rdfs:subClassOf :BuildingSpace ,
                                   [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                            owl:onProperty :isHighlyAffectedBy ;
                                                            owl:hasValue :OutdoorRelativeHumidity
                                                          ]
                                                          [ rdf:type owl:Restriction ;
                                                            owl:onProperty :isHighlyAffectedBy ;
                                                            owl:hasValue :OutdoorTemperature
                                                          ]
                                                        ) ;
                                     rdf:type owl:Class
                                   ] .


###  http://w3id.org/eepsa#BelowGroundLevelSpace
:BelowGroundLevelSpace rdf:type owl:Class ;
                       owl:equivalentClass [ owl:intersectionOf ( :BuildingSpace
                                                                  [ rdf:type owl:Restriction ;
                                                                    owl:onProperty :pset_aboveGround ;
                                                                    owl:hasValue "false"^^xsd:boolean
                                                                  ]
                                                                ) ;
                                             rdf:type owl:Class
                                           ] ;
                       rdfs:subClassOf :BuildingSpace .


###  http://w3id.org/eepsa#BlindActuator
:BlindActuator rdf:type owl:Class ;
               rdfs:subClassOf <http://www.irit.fr/recherches/MELODI/ontologies/SAN#ActuatingDevice> ;
               rdfs:comment "Actuator used to put the blinds up or down"@en .


###  http://w3id.org/eepsa#Building
:Building rdf:type owl:Class ;
          rdfs:subClassOf <http://www.w3.org/2003/01/geo/wgs84_pos#SpatialThing> ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :hasSpace ;
                            owl:someValuesFrom :BuildingSpace
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty <http://www.w3.org/2003/01/geo/wgs84_pos#location> ;
                            owl:someValuesFrom <http://www.w3.org/2003/01/geo/wgs84_pos#SpatialThing>
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :hasPostalAddress ;
                            owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                            owl:onClass :PostalAddress
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty <http://xmlns.com/foaf/0.1/name> ;
                            owl:someValuesFrom xsd:string
                          ] ;
          rdfs:comment "Definition according to ISO 6707-1: Construction work that has the provision of shelter for its occupants or contents as one of its main purpose and is normally designed to stand permanently in one place." ;
          rdfs:seeAlso "http://ifcowl.openbimstandards.org/IFC4_ADD2#IfcBuilding" .


###  http://w3id.org/eepsa#BuildingElement
:BuildingElement rdf:type owl:Class ;
                 rdfs:subClassOf <http://www.w3.org/2003/01/geo/wgs84_pos#SpatialThing> ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty :hasOrientation ;
                                   owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                   owl:onClass [ rdf:type owl:Class ;
                                                 owl:oneOf ( :eastOrientation
                                                             :northEastOrientation
                                                             :northOrientation
                                                             :northWestOrientation
                                                             :southEastOrientation
                                                             :southOrientation
                                                             :southWestOrientation
                                                             :westOrientation
                                                           )
                                               ]
                                 ] ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty :pset_isExternal ;
                                   owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                   owl:onDataRange xsd:boolean
                                 ] ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty :pset_status ;
                                   owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                   owl:onDataRange xsd:string
                                 ] ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty :pset_thermalTransmittance ;
                                   owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                   owl:onDataRange xsd:double
                                 ] ;
                 rdfs:comment "The building element comprises all elements that are primarily part of the construction of a building, i.e., its structural and space separating system." ;
                 rdfs:seeAlso <http://ifcowl.openbimstandards.org/IFC4_ADD2#IfcBuildingElement> .


###  http://w3id.org/eepsa#BuildingSpace
:BuildingSpace rdf:type owl:Class ;
               rdfs:subClassOf <http://www.w3.org/2003/01/geo/wgs84_pos#SpatialThing> ,
                               [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                        owl:onProperty :isAffectedBy ;
                                                        owl:hasValue :IndoorRelativeHumidity
                                                      ]
                                                      [ rdf:type owl:Restriction ;
                                                        owl:onProperty :isAffectedBy ;
                                                        owl:hasValue :IndoorTemperature
                                                      ]
                                                      [ rdf:type owl:Restriction ;
                                                        owl:onProperty :isAffectedBy ;
                                                        owl:hasValue :SpaceOccupancy
                                                      ]
                                                    ) ;
                                 rdf:type owl:Class
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty :containsBuildingElement ;
                                 owl:someValuesFrom :BuildingElement
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty :hasSpace ;
                                 owl:someValuesFrom :BuildingSpace
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty :containsPhysicalObject ;
                                 owl:allValuesFrom <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#PhysicalObject>
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty :isSpaceOf ;
                                 owl:allValuesFrom [ rdf:type owl:Class ;
                                                     owl:unionOf ( :Building
                                                                   :BuildingSpace
                                                                 )
                                                   ]
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty :pset_isExternal ;
                                 owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                 owl:onDataRange xsd:boolean
                               ] ;
               rdfs:comment "A space represents an area or volume bounded actually or theoretically. Spaces are areas or volumes that provide for certain functions within a building." ;
               rdfs:isDefinedBy <http://ifcowl.openbimstandards.org/IFC4_ADD2#IfcSpace> .


###  http://w3id.org/eepsa#DeviceRelocation
:DeviceRelocation rdf:type owl:Class ;
                  rdfs:subClassOf <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Method> ;
                  rdfs:comment """A method that consists in placing the sensing device in an adequate location. Following guides can be followed:

https://www.rmets.org/sites/default/files/pdf/guidelines/aws-guide.pdf

http://weatherfaqs.org.uk/node/124

http://www.conservationphysics.org/datalog/datlog6.php

http://www.weatherstations.co.uk/gooddata.htm"""@en .


###  http://w3id.org/eepsa#DeviceShelter
:DeviceShelter rdf:type owl:Class ;
               rdfs:subClassOf <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Method> ;
               rdfs:comment "A method that consists in covering the sensing device adequately to avoid erroneous observations. For example, Stevenson Screen to shield the sensing device against direct precipitation or direct sunlight."@en .


###  http://w3id.org/eepsa#Door
:Door rdf:type owl:Class ;
      rdfs:subClassOf :BuildingElement ;
      owl:disjointWith :Wall ,
                       :Window ;
      rdfs:comment "Definition according to ISO 6707-1: construction for closing an opening, intended primarily for access with hinged, pivoted or sliding operation." ;
      rdfs:seeAlso <http://ifcowl.openbimstandards.org/IFC4_ADD2#IfcDoor> .


###  http://w3id.org/eepsa#EastOrientation
:EastOrientation rdf:type owl:Class ;
                 rdfs:subClassOf :Orientation .


###  http://w3id.org/eepsa#ExternalBuildingElement
:ExternalBuildingElement rdf:type owl:Class ;
                         owl:equivalentClass [ rdf:type owl:Restriction ;
                                               owl:onProperty :pset_isExternal ;
                                               owl:hasValue "true"^^xsd:boolean
                                             ] ;
                         rdfs:subClassOf :BuildingElement ;
                         rdfs:comment "Building elements that are external (face the outside of the building)"@en .


###  http://w3id.org/eepsa#ExternalDoor
:ExternalDoor rdf:type owl:Class ;
              owl:equivalentClass [ owl:intersectionOf ( :Door
                                                         :ExternalBuildingElement
                                                       ) ;
                                    rdf:type owl:Class
                                  ] ;
              rdfs:subClassOf :ExternalBuildingElement ;
              rdfs:comment "External door (faces the outside of the building)"@en .


###  http://w3id.org/eepsa#ExternalWall
:ExternalWall rdf:type owl:Class ;
              owl:equivalentClass [ owl:intersectionOf ( :ExternalBuildingElement
                                                         :Wall
                                                       ) ;
                                    rdf:type owl:Class
                                  ] ;
              rdfs:subClassOf :Wall ;
              rdfs:comment "External wall (faces the outside of the building)"@en .


###  http://w3id.org/eepsa#ExternalWindow
:ExternalWindow rdf:type owl:Class ;
                owl:equivalentClass [ owl:intersectionOf ( :ExternalBuildingElement
                                                           :Window
                                                         ) ;
                                      rdf:type owl:Class
                                    ] ;
                rdfs:subClassOf :Window ;
                rdfs:comment "External window (faces the outside of the building)"@en .


###  http://w3id.org/eepsa#HVACActuator
:HVACActuator rdf:type owl:Class ;
              rdfs:subClassOf <http://www.irit.fr/recherches/MELODI/ontologies/SAN#ActuatingDevice> ;
              rdfs:comment "HVAC actuator for cooling or heating"@en .


###  http://w3id.org/eepsa#Indoor
:Indoor rdf:type owl:Class ;
        rdfs:subClassOf <http://www.w3.org/2003/01/geo/wgs84_pos#SpatialThing> ;
        rdfs:comment "Happening, used, or existing inside a building (by http://dictionary.cambridge.org/dictionary/english/indoor)"@en .


###  http://w3id.org/eepsa#IndoorRelativeHumidity
:IndoorRelativeHumidity rdf:type owl:Class ;
                        rdfs:subClassOf <http://purl.org/iot/vocab/m3-lite#RelativeHumidity> .


###  http://w3id.org/eepsa#IndoorTemperature
:IndoorTemperature rdf:type owl:Class ;
                   rdfs:subClassOf <http://purl.org/iot/vocab/m3-lite#Temperature> .


###  http://w3id.org/eepsa#LightActuator
:LightActuator rdf:type owl:Class ;
               rdfs:subClassOf <http://www.irit.fr/recherches/MELODI/ontologies/SAN#ActuatingDevice> ;
               rdfs:comment "Actuator used to turn the lights on or off."@en .


###  http://w3id.org/eepsa#MeteorologicalProperty
:MeteorologicalProperty rdf:type owl:Class ;
                        rdfs:subClassOf <http://www.w3.org/ns/ssn/Property> ,
                                        [ rdf:type owl:Restriction ;
                                          owl:onProperty :hasDataSource ;
                                          owl:someValuesFrom :WeatherService
                                        ] ;
                        rdfs:comment "Meteorological Variables that can be measured by weather stations (this means that they are outdoors. Maybe the name has to be changed)"@en .


###  http://w3id.org/eepsa#NaturallyEnlightenedSpace
:NaturallyEnlightenedSpace rdf:type owl:Class ;
                           owl:equivalentClass [ owl:intersectionOf ( :BuildingSpace
                                                                      [ rdf:type owl:Class ;
                                                                        owl:unionOf ( [ rdf:type owl:Restriction ;
                                                                                        owl:onProperty :containsBuildingElement ;
                                                                                        owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                                                        owl:onClass :ExternalWindow
                                                                                      ]
                                                                                      [ rdf:type owl:Restriction ;
                                                                                        owl:onProperty :containsBuildingElement ;
                                                                                        owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                                                        owl:onClass :Skylight
                                                                                      ]
                                                                                    )
                                                                      ]
                                                                    ) ;
                                                 rdf:type owl:Class
                                               ] ;
                           rdfs:subClassOf :BuildingSpace ,
                                           [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                                    owl:onProperty :isHighlyAffectedBy ;
                                                                    owl:hasValue <http://purl.org/iot/vocab/m3-lite#CloudCover>
                                                                  ]
                                                                  [ rdf:type owl:Restriction ;
                                                                    owl:onProperty :isHighlyAffectedBy ;
                                                                    owl:hasValue <http://purl.org/iot/vocab/m3-lite#SolarRadiation>
                                                                  ]
                                                                  [ rdf:type owl:Restriction ;
                                                                    owl:onProperty :isHighlyAffectedBy ;
                                                                    owl:hasValue <http://purl.org/iot/vocab/m3-lite#SunPositionDirection>
                                                                  ]
                                                                  [ rdf:type owl:Restriction ;
                                                                    owl:onProperty :isHighlyAffectedBy ;
                                                                    owl:hasValue <http://purl.org/iot/vocab/m3-lite#SunPositionElevation>
                                                                  ]
                                                                ) ;
                                             rdf:type owl:Class
                                           ] .


###  http://w3id.org/eepsa#NorthOrientation
:NorthOrientation rdf:type owl:Class ;
                  rdfs:subClassOf :Orientation .


###  http://w3id.org/eepsa#Orientation
:Orientation rdf:type owl:Class ;
             rdfs:comment "The direction an envelope element faces, i.e., the direction of a vector perpendicular to and pointing away from the surface outside of the element (Building Energy Codes Program)."@en .


###  http://w3id.org/eepsa#Outdoor
:Outdoor rdf:type owl:Class ;
         rdfs:subClassOf <http://www.w3.org/2003/01/geo/wgs84_pos#SpatialThing> ;
         rdfs:comment "Existing, happening, or done outside, rather than inside a building: (http://dictionary.cambridge.org/dictionary/english/outdoor)"@en .


###  http://w3id.org/eepsa#OutdoorRelativeHumidity
:OutdoorRelativeHumidity rdf:type owl:Class ;
                         rdfs:subClassOf <http://purl.org/iot/vocab/m3-lite#RelativeHumidity> ,
                                         :MeteorologicalProperty .


###  http://w3id.org/eepsa#OutdoorTemperature
:OutdoorTemperature rdf:type owl:Class ;
                    rdfs:subClassOf <http://purl.org/iot/vocab/m3-lite#Temperature> ,
                                    :MeteorologicalProperty .


###  http://w3id.org/eepsa#Outlier
:Outlier rdf:type owl:Class ;
         rdfs:subClassOf <http://www.w3.org/ns/ssn/Observation> ;
         rdfs:comment "Observations that stand out and do not conform to the expected behaviour" .


###  http://w3id.org/eepsa#OutlierCausedByBeamSpotlight
:OutlierCausedByBeamSpotlight rdf:type owl:Class ;
                              rdfs:subClassOf :OutlierCausedByDeviceLocation ,
                                              [ rdf:type owl:Restriction ;
                                                owl:onProperty :hasSolvingMethod ;
                                                owl:allValuesFrom :DeviceRelocation
                                              ] ;
                              rdfs:comment "An erroneous observation (e.g. temperature, illuminance) caused by a beam spotlight. For example, an observation made by a sensing device located under a light bulb"@en .


###  http://w3id.org/eepsa#OutlierCausedByDeviceError
:OutlierCausedByDeviceError rdf:type owl:Class ;
                            rdfs:subClassOf :Outlier ;
                            rdfs:comment "Outliers caused by an error on the device itself" .


###  http://w3id.org/eepsa#OutlierCausedByDeviceLocation
:OutlierCausedByDeviceLocation rdf:type owl:Class ;
                               rdfs:subClassOf :Outlier .


###  http://w3id.org/eepsa#OutlierCausedByDeviceLowBattery
:OutlierCausedByDeviceLowBattery rdf:type owl:Class ;
                                 rdfs:subClassOf :OutlierCausedByDeviceError .


###  http://w3id.org/eepsa#OutlierCausedByDeviceMalfunction
:OutlierCausedByDeviceMalfunction rdf:type owl:Class ;
                                  rdfs:subClassOf :OutlierCausedByDeviceError ;
                                  rdfs:comment "An erroneous observation caused by a malfunction suffered by the device, which might have been transient."@en .


###  http://w3id.org/eepsa#OutlierCausedByExternalConditions
:OutlierCausedByExternalConditions rdf:type owl:Class ;
                                   rdfs:subClassOf :OutlierCausedByDeviceLocation ,
                                                   [ rdf:type owl:Restriction ;
                                                     owl:onProperty :hasSolvingMethod ;
                                                     owl:allValuesFrom :DeviceRelocation
                                                   ] ;
                                   rdfs:comment "An erroneous observation (e.g. temperature, illuminance) caused by external factors. For example for being next to a source of radiant heat or cold such as windows and poorly insulated outside walls."@en .


###  http://w3id.org/eepsa#OutlierCausedByHVAC
:OutlierCausedByHVAC rdf:type owl:Class ;
                     rdfs:subClassOf :OutlierCausedByDeviceLocation ,
                                     [ rdf:type owl:Restriction ;
                                       owl:onProperty :hasSolvingMethod ;
                                       owl:allValuesFrom :DeviceRelocation
                                     ] ;
                     rdfs:comment "An erroneous observation (e.g. temperature) caused by its placement next to an activated HVAC system (whether it was heating or cooling)"@en .


###  http://w3id.org/eepsa#OutlierCausedByRain
:OutlierCausedByRain rdf:type owl:Class ;
                     rdfs:subClassOf :OutlierCausedByDeviceLocation ,
                                     [ rdf:type owl:Restriction ;
                                       owl:onProperty :hasSolvingMethod ;
                                       owl:allValuesFrom :DeviceRelocation
                                     ] ,
                                     [ rdf:type owl:Restriction ;
                                       owl:onProperty :hasSolvingMethod ;
                                       owl:allValuesFrom :DeviceShelter
                                     ] ;
                     rdfs:comment "An erroneous observation caused by rainfall."@en .


###  http://w3id.org/eepsa#OutlierCausedBySunlight
:OutlierCausedBySunlight rdf:type owl:Class ;
                         rdfs:subClassOf :OutlierCausedByDeviceLocation ,
                                         [ rdf:type owl:Restriction ;
                                           owl:onProperty :hasSolvingMethod ;
                                           owl:allValuesFrom :DeviceRelocation
                                         ] ,
                                         [ rdf:type owl:Restriction ;
                                           owl:onProperty :hasSolvingMethod ;
                                           owl:allValuesFrom :DeviceShelter
                                         ] ;
                         rdfs:comment "An erroneous observation (e.g. temperature, illuminance) caused by direct sunlight."@en .


###  http://w3id.org/eepsa#PostalAddress
:PostalAddress rdf:type owl:Class ;
               rdfs:subClassOf [ rdf:type owl:Restriction ;
                                 owl:onProperty :postalAddress_hasAddress ;
                                 owl:someValuesFrom xsd:string
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty :postalAddress_hasCountry ;
                                 owl:someValuesFrom xsd:string
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty :postalAddress_hasPostalCode ;
                                 owl:someValuesFrom xsd:string
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty :postalAddress_hasRegion ;
                                 owl:someValuesFrom xsd:string
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty :postalAddress_hasTown ;
                                 owl:someValuesFrom xsd:string
                               ] ;
               rdfs:comment "This entity represents an address for delivery of paper based mail and other postal deliveries (IFC4Add2)"@en ;
               rdfs:seeAlso <http://ifcowl.openbimstandards.org/IFC4_ADD2#IfcPostalAddress> .


###  http://w3id.org/eepsa#Roof
:Roof rdf:type owl:Class ;
      rdfs:subClassOf :ExternalBuildingElement ;
      rdfs:comment "Definition according to ISO 6707-1: construction enclosing the building from above."@en ;
      rdfs:seeAlso <http://ifcowl.openbimstandards.org/IFC4_ADD2#IfcRoof> .


###  http://w3id.org/eepsa#Skylight
:Skylight rdf:type owl:Class ;
          rdfs:subClassOf :ExternalBuildingElement ;
          rdfs:comment "Skylights are light transmitting fenestration (elements filling building envelope openings) forming all, or a portion of, the roof of a building's space for daylighting purposes. (Wikipedia)"@en .


###  http://w3id.org/eepsa#SouthOrientation
:SouthOrientation rdf:type owl:Class ;
                  rdfs:subClassOf :Orientation .


###  http://w3id.org/eepsa#SpaceOccupancy
:SpaceOccupancy rdf:type owl:Class ;
                rdfs:subClassOf <http://www.w3.org/ns/ssn/Property> .


###  http://w3id.org/eepsa#TemperatureSensor
:TemperatureSensor rdf:type owl:Class ;
                   rdfs:subClassOf <http://www.w3.org/ns/ssn/SensingDevice> ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty <http://www.w3.org/ns/ssn/observes> ;
                                     owl:allValuesFrom <http://purl.org/iot/vocab/m3-lite#Temperature>
                                   ] .


###  http://w3id.org/eepsa#Wall
:Wall rdf:type owl:Class ;
      rdfs:subClassOf :BuildingElement ;
      owl:disjointWith :Window ;
      rdfs:comment "Definition according to ISO 6707-1: vertical construction usually in masonry or in concrete which bounds or subdivides a construction works and fulfils a load bearing or retaining function." ;
      rdfs:seeAlso <http://ifcowl.openbimstandards.org/IFC4_ADD2#IfcWall> .


###  http://w3id.org/eepsa#WeatherForecasting
:WeatherForecasting rdf:type owl:Class ;
                    rdfs:subClassOf :WeatherService .


