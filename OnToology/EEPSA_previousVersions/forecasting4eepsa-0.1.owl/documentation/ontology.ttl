###  https://w3id.org/forecasting4eepsa#TemperatureForecast
:TemperatureForecast rdf:type owl:Class ;
                     rdfs:subClassOf <https://w3id.org/seas/Forecast> ,
                                     [ rdf:type owl:Restriction ;
                                       owl:onProperty <http://www.w3.org/2003/01/geo/wgs84_pos#location> ;
                                       owl:someValuesFrom <http://www.w3.org/2003/01/geo/wgs84_pos#SpatialThing>
                                     ] ,
                                     [ rdf:type owl:Restriction ;
                                       owl:onProperty <https://w3id.org/pep/hasResult> ;
                                       owl:someValuesFrom :TemperatureForecastResult
                                     ] ,
                                     [ rdf:type owl:Restriction ;
                                       owl:onProperty <https://w3id.org/pep/madeBy> ;
                                       owl:someValuesFrom :TemperatureForecaster
                                     ] ,
                                     [ rdf:type owl:Restriction ;
                                       owl:onProperty <https://w3id.org/pep/usedProcedure> ;
                                       owl:someValuesFrom :TemperatureForecasting
                                     ] ,
                                     [ rdf:type owl:Restriction ;
                                       owl:onProperty <https://w3id.org/seas/forecastsProperty> ;
                                       owl:someValuesFrom <http://purl.org/iot/vocab/m3-lite#Temperature>
                                     ] ,
                                     [ rdf:type owl:Restriction ;
                                       owl:onProperty <http://www.w3.org/ns/prov#generatedAtTime> ;
                                       owl:someValuesFrom xsd:dateTime
                                     ] ;
                     rdfs:comment "A Temperature Forecast is the execution of some Temperature Forecasting process by some Temperature Forecaster."@en ;
                     rdfs:label "Temperature Forecast"@en .


###  https://w3id.org/forecasting4eepsa#TemperatureForecastResult
:TemperatureForecastResult rdf:type owl:Class ;
                           rdfs:subClassOf [ rdf:type owl:Restriction ;
                                             owl:onProperty <http://qudt.org/schema/qudt#unit> ;
                                             owl:someValuesFrom <http://qudt.org/schema/qudt#Unit>
                                           ] ,
                                           [ rdf:type owl:Restriction ;
                                             owl:onProperty <http://qudt.org/schema/qudt#numericValue> ;
                                             owl:someValuesFrom xsd:double
                                           ] ,
                                           [ rdf:type owl:Restriction ;
                                             owl:onProperty :hasForecastDateTime ;
                                             owl:someValuesFrom xsd:dateTime
                                           ] ;
                           rdfs:comment "Temperature Forecast Result represents the result of a Temperature Forecast."@en ;
                           rdfs:label "Temperature Forecast Result"@en .


###  https://w3id.org/forecasting4eepsa#TemperatureForecaster
:TemperatureForecaster rdf:type owl:Class ;
                       rdfs:subClassOf <https://w3id.org/seas/Forecaster> ,
                                       [ rdf:type owl:Restriction ;
                                         owl:onProperty <https://w3id.org/pep/implements> ;
                                         owl:someValuesFrom :TemperatureForecasting
                                       ] ,
                                       [ rdf:type owl:Restriction ;
                                         owl:onProperty <https://w3id.org/seas/forecastsProperty> ;
                                         owl:someValuesFrom <http://purl.org/iot/vocab/m3-lite#Temperature>
                                       ] ;
                       rdfs:comment "A Temperature Forecaster implements some Temperature Forecasting process, and may generate temperature forecasts."@en ;
                       rdfs:label "Temperature Forecaster"@en .


###  https://w3id.org/forecasting4eepsa#TemperatureForecasting
:TemperatureForecasting rdf:type owl:Class ;
                        rdfs:subClassOf <https://w3id.org/seas/Forecasting> ,
                                        [ rdf:type owl:Restriction ;
                                          owl:onProperty <https://w3id.org/seas/forecastsProperty> ;
                                          owl:someValuesFrom <http://purl.org/iot/vocab/m3-lite#Temperature>
                                        ] ;
                        rdfs:comment "Temperature Forecasting is the process of forecasting the temperature."@en ;
                        rdfs:label "Temperature Forecasting"@en .


