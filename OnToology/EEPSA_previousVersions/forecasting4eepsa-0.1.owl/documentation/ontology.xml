    <!-- https://w3id.org/forecasting4eepsa#TemperatureForecast -->


    <owl:Class rdf:about="https://w3id.org/forecasting4eepsa#TemperatureForecast">
        <rdfs:subClassOf rdf:resource="https://w3id.org/seas/Forecast"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.w3.org/2003/01/geo/wgs84_pos#location"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/2003/01/geo/wgs84_pos#SpatialThing"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/pep/hasResult"/>
                <owl:someValuesFrom rdf:resource="https://w3id.org/forecasting4eepsa#TemperatureForecastResult"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/pep/madeBy"/>
                <owl:someValuesFrom rdf:resource="https://w3id.org/forecasting4eepsa#TemperatureForecaster"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/pep/usedProcedure"/>
                <owl:someValuesFrom rdf:resource="https://w3id.org/forecasting4eepsa#TemperatureForecasting"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/seas/forecastsProperty"/>
                <owl:someValuesFrom rdf:resource="http://purl.org/iot/vocab/m3-lite#Temperature"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.w3.org/ns/prov#generatedAtTime"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">A Temperature Forecast is the execution of some Temperature Forecasting process by some Temperature Forecaster.</rdfs:comment>
        <rdfs:label xml:lang="en">Temperature Forecast</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/forecasting4eepsa#TemperatureForecastResult -->


    <owl:Class rdf:about="https://w3id.org/forecasting4eepsa#TemperatureForecastResult">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://qudt.org/schema/qudt#unit"/>
                <owl:someValuesFrom rdf:resource="http://qudt.org/schema/qudt#Unit"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://qudt.org/schema/qudt#numericValue"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#double"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/forecasting4eepsa#hasForecastDateTime"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">Temperature Forecast Result represents the result of a Temperature Forecast.</rdfs:comment>
        <rdfs:label xml:lang="en">Temperature Forecast Result</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/forecasting4eepsa#TemperatureForecaster -->


    <owl:Class rdf:about="https://w3id.org/forecasting4eepsa#TemperatureForecaster">
        <rdfs:subClassOf rdf:resource="https://w3id.org/seas/Forecaster"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/pep/implements"/>
                <owl:someValuesFrom rdf:resource="https://w3id.org/forecasting4eepsa#TemperatureForecasting"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/seas/forecastsProperty"/>
                <owl:someValuesFrom rdf:resource="http://purl.org/iot/vocab/m3-lite#Temperature"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">A Temperature Forecaster implements some Temperature Forecasting process, and may generate temperature forecasts.</rdfs:comment>
        <rdfs:label xml:lang="en">Temperature Forecaster</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/forecasting4eepsa#TemperatureForecasting -->


    <owl:Class rdf:about="https://w3id.org/forecasting4eepsa#TemperatureForecasting">
        <rdfs:subClassOf rdf:resource="https://w3id.org/seas/Forecasting"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/seas/forecastsProperty"/>
                <owl:someValuesFrom rdf:resource="http://purl.org/iot/vocab/m3-lite#Temperature"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">Temperature Forecasting is the process of forecasting the temperature.</rdfs:comment>
        <rdfs:label xml:lang="en">Temperature Forecasting</rdfs:label>
    </owl:Class>
