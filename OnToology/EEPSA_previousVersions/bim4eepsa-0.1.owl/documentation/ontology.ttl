###  http://www.w3.org/2003/01/geo/wgs84_pos#SpatialThing
<http://www.w3.org/2003/01/geo/wgs84_pos#SpatialThing> rdf:type owl:Class ;
                                                       rdfs:comment "Anything with spatial extent, i.e. size, shape, or position. e.g. people, places, bowling balls, as well as abstract areas like cubes."@en ;
                                                       rdfs:isDefinedBy <http://www.w3.org/2003/01/geo/wgs84_pos#> ;
                                                       rdfs:label "Spatial thing"@en .


###  https://w3id.org/bim4eepsa#BuildingElement
:BuildingElement rdf:type owl:Class ;
                 rdfs:subClassOf <https://w3id.org/def/saref4bldg#PhysicalObject> ;
                 rdfs:comment "The building element comprises all elements that are primarily part of the construction of a building, i.e., its structural and space separating system."@en ;
                 rdfs:seeAlso <http://ifcowl.openbimstandards.org/IFC4_ADD2#IfcBuildingElement> .


###  https://w3id.org/bim4eepsa#Door
:Door rdf:type owl:Class ;
      rdfs:subClassOf :BuildingElement ;
      rdfs:comment "Definition according to ISO 6707-1: construction for closing an opening, intended primarily for access with hinged, pivoted or sliding operation."@en ;
      rdfs:seeAlso <http://ifcowl.openbimstandards.org/IFC4_ADD2#IfcDoor> .


###  https://w3id.org/bim4eepsa#ExternalBuildingElement
:ExternalBuildingElement rdf:type owl:Class ;
                         rdfs:subClassOf :BuildingElement ;
                         rdfs:seeAlso "Building elements that are external (face the outside of the building)"@en .


###  https://w3id.org/bim4eepsa#ExternalDoor
:ExternalDoor rdf:type owl:Class ;
              owl:equivalentClass [ owl:intersectionOf ( :Door
                                                         :ExternalBuildingElement
                                                       ) ;
                                    rdf:type owl:Class
                                  ] ;
              rdfs:subClassOf :Door ;
              rdfs:label "External door"@en ;
              rdfs:seeAlso "External door (faces the outside of the building)"@en .


###  https://w3id.org/bim4eepsa#ExternalWall
:ExternalWall rdf:type owl:Class ;
              owl:equivalentClass [ owl:intersectionOf ( :ExternalBuildingElement
                                                         :Wall
                                                       ) ;
                                    rdf:type owl:Class
                                  ] ;
              rdfs:subClassOf :Wall ;
              rdfs:label "External wall"@en ;
              rdfs:seeAlso "External wall (faces the outside of the building)"@en .


###  https://w3id.org/bim4eepsa#ExternalWindow
:ExternalWindow rdf:type owl:Class ;
                owl:equivalentClass [ owl:intersectionOf ( :ExternalBuildingElement
                                                           :Window
                                                         ) ;
                                      rdf:type owl:Class
                                    ] ;
                rdfs:subClassOf :Window ;
                rdfs:label "External window"@en ;
                rdfs:seeAlso "External window (faces the outside of the building)"@en .


###  https://w3id.org/bim4eepsa#Roof
:Roof rdf:type owl:Class ;
      rdfs:subClassOf :BuildingElement ;
      rdfs:comment "Definition according to ISO 6707-1: construction enclosing the building from above."@en ;
      rdfs:seeAlso <http://ifcowl.openbimstandards.org/IFC4_ADD2#IfcRoof> .


###  https://w3id.org/bim4eepsa#Skylight
:Skylight rdf:type owl:Class ;
          rdfs:subClassOf :BuildingElement ;
          rdfs:seeAlso "Skylights are light transmitting fenestration (elements filling building envelope openings) forming all, or a portion of, the roof of a building's space for daylighting purposes. (Wikipedia)"@en .


###  https://w3id.org/bim4eepsa#Wall
:Wall rdf:type owl:Class ;
      rdfs:subClassOf :BuildingElement ;
      rdfs:comment "Definition according to ISO 6707-1: vertical construction usually in masonry or in concrete which bounds or subdivides a construction works and fulfils a load bearing or retaining function."@en ;
      rdfs:seeAlso <http://ifcowl.openbimstandards.org/IFC4_ADD2#IfcWall> .


###  https://w3id.org/bim4eepsa#WeatherStation
:WeatherStation rdf:type owl:Class ;
                rdfs:subClassOf <https://w3id.org/def/saref4bldg#Building> ;
                rdfs:comment "A weather station is a facility, either on land or sea, with instruments and equipment for measuring atmospheric conditions to provide information for weather forecasts and to study the weather and climate (Wikipedia)"@en ;
                rdfs:label <http://aemet.linkeddata.es/ontology/WeatherStation> ,
                           "Weather station"@en .


###  https://w3id.org/bim4eepsa#Window
:Window rdf:type owl:Class ;
        rdfs:subClassOf :BuildingElement ;
        rdfs:comment "Definition according to ISO 6707-1: Construction for closing a vertical or near vertical opening in a wall or pitched roof that will admit light and may admit fresh air."@en ;
        rdfs:seeAlso <http://ifcowl.openbimstandards.org/IFC4_ADD2#IfcWindow> .


###  https://w3id.org/def/saref4bldg#Building
<https://w3id.org/def/saref4bldg#Building> rdf:type owl:Class ;
                                           rdfs:subClassOf <http://www.w3.org/2003/01/geo/wgs84_pos#SpatialThing> ,
                                                           [ rdf:type owl:Restriction ;
                                                             owl:onProperty <http://www.w3.org/2003/01/geo/wgs84_pos#location> ;
                                                             owl:allValuesFrom <http://www.w3.org/2003/01/geo/wgs84_pos#SpatialThing>
                                                           ] ,
                                                           [ rdf:type owl:Restriction ;
                                                             owl:onProperty <https://w3id.org/def/saref4bldg#hasSpace> ;
                                                             owl:allValuesFrom <https://w3id.org/def/saref4bldg#BuildingSpace>
                                                           ] ,
                                                           [ rdf:type owl:Restriction ;
                                                             owl:onProperty :pset_BuildingCommon_buildingID ;
                                                             owl:allValuesFrom xsd:string
                                                           ] ,
                                                           [ rdf:type owl:Restriction ;
                                                             owl:onProperty :pset_BuildingCommon_numberOfStoreys ;
                                                             owl:allValuesFrom xsd:integer
                                                           ] ,
                                                           [ rdf:type owl:Restriction ;
                                                             owl:onProperty :pset_BuildingCommon_occupancyType ;
                                                             owl:allValuesFrom xsd:string
                                                           ] ;
                                           rdfs:comment "A building represents a structure that provides shelter for its occupants or contents and stands in one place. The building is also used to provide a basic element within the spatial structure hierarchy for the components of a building project (together with site, storey, and space)."@en ;
                                           rdfs:label "Building"@en ;
                                           rdfs:seeAlso <https://w3id.org/ifc/IFC4_ADD1#IfcBuilding> ;
                                           <http://www.w3.org/ns/prov#hadPrimarySource> <http://www.buildingsmart-tech.org/ifc/IFC4/Add1/html/schema/ifcproductextension/lexical/ifcbuilding.htm> .


###  https://w3id.org/def/saref4bldg#BuildingSpace
<https://w3id.org/def/saref4bldg#BuildingSpace> rdf:type owl:Class ;
                                                rdfs:subClassOf <http://www.w3.org/2003/01/geo/wgs84_pos#SpatialThing> ,
                                                                [ rdf:type owl:Restriction ;
                                                                  owl:onProperty <https://w3id.org/def/saref4bldg#contains> ;
