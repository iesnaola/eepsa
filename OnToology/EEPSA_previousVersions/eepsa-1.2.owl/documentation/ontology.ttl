@prefix : <https://w3id.org/eepsa#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <https://w3id.org/eepsa> .

<https://w3id.org/eepsa> rdf:type owl:Ontology ;
                          owl:imports <http://www.w3.org/2006/time> ,
                                      <http://www.w3.org/ns/ssn/> ,
                                      <https://w3id.org/bim4eepsa> ,
                                      <https://w3id.org/measurements4eepsa> ;
                          <http://purl.org/dc/elements/1.1/publisher> "IK4-TEKNIKER (http://www.tekniker.es)" ;
                          <http://purl.org/dc/elements/1.1/title> "EEPSA (Energy Efficiency Prediction Semantic Assistant) Ontology" ;
                          <http://purl.org/dc/terms/created> "2016-02-20" ;
                          <http://purl.org/dc/terms/creator> """Iker Esnaola-Gonzalez <mailto:iker.esnaola@tekniker.es>
Jesús Bermúdez <mailto:jesus.bermudez@ehu.eus>"""@en ;
                          <http://purl.org/dc/terms/issued> "2017-07-21" ;
                          <http://purl.org/dc/terms/license> "This work is licensed under the Creative Commons Attribution-NonCommercial 4.0 International License. To view a copy of this license, visit http://creativecommons.org/licenses/by-nc/4.0/."@en ;
                          <http://purl.org/dc/terms/modified> "2017-07-06" ;
                          <http://purl.org/ontology/bibo/status> "Work in progress"@en ;
                          <http://purl.org/vocab/vann/preferredNamespacePrefix> "eepsa" ;
                          owl:versionInfo 1.2 .

#################################################################
#    Annotation properties
#################################################################

###  http://purl.oclc.org/NET/UNIS/iot-lite/iot-lite#relativeLocation
<http://purl.oclc.org/NET/UNIS/iot-lite/iot-lite#relativeLocation> rdf:type owl:AnnotationProperty ;
                                                                   rdfs:comment "A location not provided by geo-coordinates (i.e. building A, city of Aarhus)."@en ;
                                                                   rdfs:isDefinedBy <http://purl.oclc.org/NET/UNIS/fiware/iot-lite#> ;
                                                                   rdfs:domain <http://www.w3.org/2003/01/geo/wgs84_pos#Point> .


###  http://purl.org/dc/elements/1.1/date
<http://purl.org/dc/elements/1.1/date> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/description
<http://purl.org/dc/elements/1.1/description> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/title
<http://purl.org/dc/elements/1.1/title> rdf:type owl:AnnotationProperty .


###  http://purl.org/ontology/bibo/status
<http://purl.org/ontology/bibo/status> rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2003/01/geo/wgs84_pos#alt
<http://www.w3.org/2003/01/geo/wgs84_pos#alt> rdf:type owl:AnnotationProperty ;
                                              rdfs:comment """The WGS84 altitude of a SpatialThing (decimal meters 
above the local reference ellipsoid).""" ;
                                              rdfs:label "altitude" ;
                                              rdfs:domain <http://www.w3.org/2003/01/geo/wgs84_pos#SpatialThing> .


###  http://www.w3.org/2003/01/geo/wgs84_pos#lat
<http://www.w3.org/2003/01/geo/wgs84_pos#lat> rdf:type owl:AnnotationProperty ;
                                              rdfs:comment "The WGS84 latitude of a SpatialThing (decimal degrees)." ;
                                              rdfs:label "latitude" ;
                                              rdfs:domain <http://www.w3.org/2003/01/geo/wgs84_pos#SpatialThing> .


###  http://www.w3.org/2003/01/geo/wgs84_pos#location
<http://www.w3.org/2003/01/geo/wgs84_pos#location> rdf:type owl:AnnotationProperty ;
                                                   rdfs:subPropertyOf <http://xmlns.com/foaf/0.1/based_near> ;
                                                   rdfs:range <http://www.w3.org/2003/01/geo/wgs84_pos#SpatialThing> .


###  http://www.w3.org/2003/01/geo/wgs84_pos#long
<http://www.w3.org/2003/01/geo/wgs84_pos#long> rdf:type owl:AnnotationProperty ;
                                               rdfs:comment "The WGS84 longitude of a SpatialThing (decimal degrees)." ;
                                               rdfs:label "longitude" ;
                                               rdfs:domain <http://www.w3.org/2003/01/geo/wgs84_pos#SpatialThing> .


###  http://xmlns.com/foaf/0.1/based_near
<http://xmlns.com/foaf/0.1/based_near> rdf:type owl:AnnotationProperty .


###  http://xmlns.com/foaf/0.1/depiction
<http://xmlns.com/foaf/0.1/depiction> rdf:type owl:AnnotationProperty .


###  http://xmlns.com/foaf/0.1/name
<http://xmlns.com/foaf/0.1/name> rdfs:range rdfs:Literal .


#################################################################
#    Object Properties
#################################################################

###  http://www.w3.org/2003/01/geo/wgs84_pos#location
<http://www.w3.org/2003/01/geo/wgs84_pos#location> rdf:type owl:ObjectProperty .


###  http://xmlns.com/foaf/0.1/name
<http://xmlns.com/foaf/0.1/name> rdf:type owl:ObjectProperty ;
                                 rdfs:domain owl:Thing .


###  https://w3id.org/def/saref4bldg#contains
<https://w3id.org/def/saref4bldg#contains> rdf:type owl:ObjectProperty .


###  https://w3id.org/def/saref4bldg#isContainedIn
<https://w3id.org/def/saref4bldg#isContainedIn> rdf:type owl:ObjectProperty .


###  https://w3id.org/eepsa#capturedIn
:capturedIn rdf:type owl:ObjectProperty ;
            owl:inverseOf :captures ;
            owl:propertyChainAxiom ( :containsPhysicalObject
                                     [ owl:inverseOf <http://www.w3.org/ns/sosa/isObservedBy>
                                     ]
                                   ) ;
            rdfs:comment "Relation between an Observation and the Spatial Thing (Building, Space) where it has been captured"@en ;
            rdfs:label "captured in"@en .


###  https://w3id.org/eepsa#captures
:captures rdf:type owl:ObjectProperty ;
          rdfs:comment "Relation between a Spatial Thing (Building, Space) and the Observation that has been captured"@en ;
          rdfs:label "captures"@en .


###  https://w3id.org/eepsa#containsBuildingElement
:containsBuildingElement rdf:type owl:ObjectProperty ;
                         rdfs:subPropertyOf <https://w3id.org/def/saref4bldg#contains> ;
                         owl:inverseOf :isContainedInSpatialStructure ;
                         rdfs:comment "Relation used to assign Building Element to a certain Spatial Structure (Space or Building)." .


###  https://w3id.org/eepsa#containsPhysicalObject
:containsPhysicalObject rdf:type owl:ObjectProperty ;
                        rdfs:subPropertyOf <https://w3id.org/def/saref4bldg#contains> ;
                        owl:inverseOf :isContainedInPhysicalSpace ;
                        rdfs:comment "A relation between a physical space and the objects located in such space." ;
                        rdfs:seeAlso <https://w3id.org/def/saref4bldg#contains> .


###  https://w3id.org/eepsa#hasDataSource
:hasDataSource rdf:type owl:ObjectProperty ;
               rdfs:comment "Relation between a Thing and a data source where values can be retrieved."@en ;
               rdfs:label "has data source"@en .


###  https://w3id.org/eepsa#hasSolvingMethod
:hasSolvingMethod rdf:type owl:ObjectProperty ;
                  rdfs:range <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Method> ;
                  rdfs:comment "A relation between a Thing (mainly an Outlier) and a Method proposed to solve it" ;
                  rdfs:label "has solving method"@en .


###  https://w3id.org/eepsa#hasSunExposurePeriod
:hasSunExposurePeriod rdf:type owl:ObjectProperty ;
                      rdfs:comment "Determines the period to which it is exposed to sun" ;
                      rdfs:label "has sun exposure period"@en .


###  https://w3id.org/eepsa#hasUnitOfMeasure
:hasUnitOfMeasure rdf:type owl:ObjectProperty ;
                  rdfs:range <http://qudt.org/schema/qudt#Unit> ;
                  rdfs:comment "Relation between a Thing (Sensor) and a Unit of measurement"@en ;
                  rdfs:label "has unit of measure"@en .


###  https://w3id.org/eepsa#isAffectedBy
:isAffectedBy rdf:type owl:ObjectProperty ;
              rdfs:domain <https://w3id.org/def/saref4bldg#BuildingSpace> ;
              rdfs:range <http://www.w3.org/ns/ssn/Property> ;
              rdfs:comment "Relationship between a space and the Quantity Kind that is affects its indoor conditions" ;
              rdfs:label "is affected by"@en .


###  https://w3id.org/eepsa#isContainedInPhysicalSpace
:isContainedInPhysicalSpace rdf:type owl:ObjectProperty ;
                            rdfs:subPropertyOf <https://w3id.org/def/saref4bldg#isContainedIn> ;
                            rdfs:comment "A relation between an object and the physical space in which it is located." ;
                            rdfs:seeAlso <https://w3id.org/def/saref4bldg#isContainedIn> .


###  https://w3id.org/eepsa#isContainedInSpatialStructure
:isContainedInSpatialStructure rdf:type owl:ObjectProperty ;
                               rdfs:subPropertyOf <https://w3id.org/def/saref4bldg#isContainedIn> ;
                               rdfs:comment "A relation between a Building Element and the Spatial Structure (Space or Building) in which it is contained"@en ;
                               rdfs:isDefinedBy <http://ifcowl.openbimstandards.org/IFC4_ADD2#IfcRelContainedInSpatialStructure> .


###  https://w3id.org/eepsa#isHighlyAffectedBy
:isHighlyAffectedBy rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf :isAffectedBy ;
                    rdfs:range <http://www.w3.org/ns/ssn/Property> ;
                    rdfs:comment "Relationship between a space and the Quantity Kind that is affects its indoor conditions with a higher degree" .


###  https://w3id.org/eepsa#providesWeatherService
:providesWeatherService rdf:type owl:ObjectProperty ;
                        rdfs:range :WeatherService ;
                        rdfs:comment "Relation between a Weather Station and the Weather Service it provides (weather report, weather forecast,...)"@en ;
                        rdfs:label "provides weather service"@en .


###  https://w3id.org/eepsa#susceptibleToOutliersCausedBy
:susceptibleToOutliersCausedBy rdf:type owl:ObjectProperty ;
                               rdfs:comment "Determines the cause that makes a sensing devices susceptible to suffer from Outliers." ;
                               rdfs:label "susceptible to outliers caused by"@en .


#################################################################
#    Data properties
#################################################################

###  http://xmlns.com/foaf/0.1/name
<http://xmlns.com/foaf/0.1/name> rdf:type owl:DatatypeProperty .


###  https://w3id.org/eepsa#endingTime
:endingTime rdf:type owl:DatatypeProperty ;
            rdfs:comment "Determines the Ending Time of a temporal entity."@en ;
            rdfs:label "ending time"@en .


###  https://w3id.org/eepsa#hasMonth
:hasMonth rdf:type owl:DatatypeProperty ;
          rdfs:label "has month"@en .


###  https://w3id.org/eepsa#manufacturedBy
:manufacturedBy rdf:type owl:DatatypeProperty ;
                rdfs:range xsd:string ;
                rdfs:comment "Determines the manufacturer of a Thing"@en ;
                rdfs:label "manufactured by"@en ;
                <http://www.w3.org/2004/02/skos/core#example> """Example:
myOnto:Vehicle1 eepsa:manufacturedBy xsd:string(\"Volkswagen\")."""@en .


###  https://w3id.org/eepsa#model
:model rdf:type owl:DatatypeProperty ;
       rdfs:range xsd:string ;
       rdfs:comment "Model of a Thing (normally a Physical Object)" ;
       rdfs:label "model"@en ;
       <http://www.w3.org/2004/02/skos/core#example> """Example:
myOnto:Vehicle1 eepsa:model xsd:string(\"Golf GTI\")."""@en .


###  https://w3id.org/eepsa#obsDate
:obsDate rdf:type owl:DatatypeProperty ;
         rdfs:comment "Determines the Date (e.g. yyyy-MM-dd) of an observation." ;
         rdfs:label "observation date"@en ;
         <http://www.w3.org/2004/02/skos/core#example> """Example:
myOnto:obs1 eepsa:observationDate \"2017-02-15\"."""@en .


###  https://w3id.org/eepsa#obsTime
:obsTime rdf:type owl:DatatypeProperty ;
         rdfs:comment "Determines the Time (e.g. hh:mm:ss) of an observation." ;
         rdfs:label "observation time"@en ;
         <http://www.w3.org/2004/02/skos/core#example> """Example:
myOnto:obs1 eepsa:observationTime \"12:00:00\"."""@en .


###  https://w3id.org/eepsa#startingTime
:startingTime rdf:type owl:DatatypeProperty ;
              rdfs:comment "Determines the Starting time of a temporal entity."@en ;
              rdfs:label "starting time"@en .


#################################################################
#    Classes
#################################################################

###  http://purl.org/iot/vocab/m3-lite#AirPollution
<http://purl.org/iot/vocab/m3-lite#AirPollution> rdfs:subClassOf :MeteorologicalProperty .


###  http://purl.org/iot/vocab/m3-lite#AirQuality
<http://purl.org/iot/vocab/m3-lite#AirQuality> rdfs:subClassOf :MeteorologicalProperty .


###  http://purl.org/iot/vocab/m3-lite#AirTemperature
<http://purl.org/iot/vocab/m3-lite#AirTemperature> rdfs:subClassOf :MeteorologicalProperty .


###  http://purl.org/iot/vocab/m3-lite#CO2
<http://purl.org/iot/vocab/m3-lite#CO2> rdfs:subClassOf :MeteorologicalProperty .


###  http://purl.org/iot/vocab/m3-lite#ChemicalAgentAtmosphericConcentration
<http://purl.org/iot/vocab/m3-lite#ChemicalAgentAtmosphericConcentration> rdfs:subClassOf :MeteorologicalProperty .


###  http://purl.org/iot/vocab/m3-lite#CloudCover
<http://purl.org/iot/vocab/m3-lite#CloudCover> rdfs:subClassOf :MeteorologicalProperty .


###  http://purl.org/iot/vocab/m3-lite#DewPoint
<http://purl.org/iot/vocab/m3-lite#DewPoint> rdfs:subClassOf :MeteorologicalProperty .


###  http://purl.org/iot/vocab/m3-lite#HumiditySensor
<http://purl.org/iot/vocab/m3-lite#HumiditySensor> rdfs:subClassOf [ rdf:type owl:Restriction ;
                                                                     owl:onProperty :susceptibleToOutliersCausedBy ;
                                                                     owl:hasValue <http://purl.org/iot/vocab/m3-lite#Rainfall>
                                                                   ] .


###  http://purl.org/iot/vocab/m3-lite#Illuminance
<http://purl.org/iot/vocab/m3-lite#Illuminance> rdfs:subClassOf :MeteorologicalProperty .


###  http://purl.org/iot/vocab/m3-lite#LeafWetness
<http://purl.org/iot/vocab/m3-lite#LeafWetness> rdfs:subClassOf :MeteorologicalProperty .


###  http://purl.org/iot/vocab/m3-lite#Precipitation
<http://purl.org/iot/vocab/m3-lite#Precipitation> rdfs:subClassOf :MeteorologicalProperty .


###  http://purl.org/iot/vocab/m3-lite#Rainfall
<http://purl.org/iot/vocab/m3-lite#Rainfall> rdfs:subClassOf :MeteorologicalProperty .


###  http://purl.org/iot/vocab/m3-lite#SolarRadiation
<http://purl.org/iot/vocab/m3-lite#SolarRadiation> rdfs:subClassOf :MeteorologicalProperty .


###  http://purl.org/iot/vocab/m3-lite#SunPositionDirection
<http://purl.org/iot/vocab/m3-lite#SunPositionDirection> rdfs:subClassOf :MeteorologicalProperty .


###  http://purl.org/iot/vocab/m3-lite#SunPositionElevation
<http://purl.org/iot/vocab/m3-lite#SunPositionElevation> rdfs:subClassOf :MeteorologicalProperty .


###  http://purl.org/iot/vocab/m3-lite#Thermometer
<http://purl.org/iot/vocab/m3-lite#Thermometer> owl:equivalentClass :TemperatureSensor .


###  http://purl.org/iot/vocab/m3-lite#WindChill
<http://purl.org/iot/vocab/m3-lite#WindChill> rdfs:subClassOf :MeteorologicalProperty .


###  http://purl.org/iot/vocab/m3-lite#WindDirection
<http://purl.org/iot/vocab/m3-lite#WindDirection> rdfs:subClassOf :MeteorologicalProperty .


###  http://purl.org/iot/vocab/m3-lite#WindSpeed
<http://purl.org/iot/vocab/m3-lite#WindSpeed> rdfs:subClassOf :MeteorologicalProperty .


###  http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Method
<http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Method> rdf:type owl:Class ;
                                                               rdfs:comment "A method is a Description that defines or uses concepts in order to guide carrying out actions aimed at a solution with respect to a problem. It is different from a Plan, because plans could be carried out in order to follow a method, but a method can be followed by executing alternative plans." ;
                                                               rdfs:isDefinedBy <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl> .


###  http://www.w3.org/2003/01/geo/wgs84_pos#Point
<http://www.w3.org/2003/01/geo/wgs84_pos#Point> rdf:type owl:Class ;
                                                rdfs:comment """ 
Uniquely identified by lat/long/alt. i.e.

spaciallyIntersects(P1, P2) :- lat(P1, LAT), long(P1, LONG), alt(P1, ALT),
  lat(P2, LAT), long(P2, LONG), alt(P2, ALT).

sameThing(P1, P2) :- type(P1, Point), type(P2, Point), spaciallyIntersects(P1, P2).
  """ ,
                                                             """A point, typically described using a coordinate system relative to Earth, such as WGS84.
  """ ;
                                                rdfs:label "point" .


###  http://www.w3.org/2003/01/geo/wgs84_pos#SpatialThing
<http://www.w3.org/2003/01/geo/wgs84_pos#SpatialThing> rdf:type owl:Class .


###  http://www.w3.org/ns/sosa/Sensor
<http://www.w3.org/ns/sosa/Sensor> rdfs:subClassOf [ rdf:type owl:Restriction ;
                                                     owl:onProperty :hasUnitOfMeasure ;
                                                     owl:someValuesFrom <http://qudt.org/schema/qudt#Unit>
                                                   ] .


###  https://w3id.org/bim4eepsa#WeatherStation
<https://w3id.org/bim4eepsa#WeatherStation> rdfs:subClassOf [ rdf:type owl:Restriction ;
                                                              owl:onProperty :providesWeatherService ;
                                                              owl:someValuesFrom :WeatherService
                                                            ] .


###  https://w3id.org/def/saref4bldg#BuildingSpace
<https://w3id.org/def/saref4bldg#BuildingSpace> rdf:type owl:Class ;
                                                rdfs:subClassOf [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                                                         owl:onProperty :isAffectedBy ;
                                                                                         owl:hasValue :SpaceOccupancy
                                                                                       ]
                                                                                       [ rdf:type owl:Restriction ;
                                                                                         owl:onProperty :isAffectedBy ;
                                                                                         owl:hasValue <https://w3id.org/measurements4eepsa#IndoorRelativeHumidity>
                                                                                       ]
                                                                                       [ rdf:type owl:Restriction ;
                                                                                         owl:onProperty :isAffectedBy ;
                                                                                         owl:hasValue <https://w3id.org/measurements4eepsa#IndoorTemperature>
                                                                                       ]
                                                                                     ) ;
                                                                  rdf:type owl:Class
                                                                ] .


###  https://w3id.org/def/saref4bldg#PhysicalObject
<https://w3id.org/def/saref4bldg#PhysicalObject> rdf:type owl:Class .


###  https://w3id.org/eepsa#AdjacentToOutdoorSpace
:AdjacentToOutdoorSpace rdf:type owl:Class ;
                        owl:equivalentClass [ owl:intersectionOf ( <https://w3id.org/def/saref4bldg#BuildingSpace>
                                                                   [ rdf:type owl:Restriction ;
                                                                     owl:onProperty <https://w3id.org/def/saref4bldg#contains> ;
                                                                     owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                                     owl:onClass <https://w3id.org/bim4eepsa#ExternalBuildingElement>
                                                                   ]
                                                                 ) ;
                                              rdf:type owl:Class
                                            ] ;
                        rdfs:subClassOf <https://w3id.org/def/saref4bldg#BuildingSpace> ,
                                        [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                                 owl:onProperty :isAffectedBy ;
                                                                 owl:hasValue <http://purl.org/iot/vocab/m3-lite#CloudCover>
                                                               ]
                                                               [ rdf:type owl:Restriction ;
                                                                 owl:onProperty :isAffectedBy ;
                                                                 owl:hasValue <http://purl.org/iot/vocab/m3-lite#SolarRadiation>
                                                               ]
                                                               [ rdf:type owl:Restriction ;
                                                                 owl:onProperty :isAffectedBy ;
                                                                 owl:hasValue <http://purl.org/iot/vocab/m3-lite#SunPositionDirection>
                                                               ]
                                                               [ rdf:type owl:Restriction ;
                                                                 owl:onProperty :isAffectedBy ;
                                                                 owl:hasValue <http://purl.org/iot/vocab/m3-lite#SunPositionElevation>
                                                               ]
                                                               [ rdf:type owl:Restriction ;
                                                                 owl:onProperty :isAffectedBy ;
                                                                 owl:hasValue <http://purl.org/iot/vocab/m3-lite#WindSpeed>
                                                               ]
                                                               [ rdf:type owl:Restriction ;
                                                                 owl:onProperty :isAffectedBy ;
                                                                 owl:hasValue <https://w3id.org/measurements4eepsa#OutdoorRelativeHumidity>
                                                               ]
                                                               [ rdf:type owl:Restriction ;
                                                                 owl:onProperty :isAffectedBy ;
                                                                 owl:hasValue <https://w3id.org/measurements4eepsa#OutdoorTemperature>
                                                               ]
                                                             ) ;
                                          rdf:type owl:Class
                                        ] ;
                        rdfs:comment "A space in contact with the exterior by means of at least one building element."@en ;
                        rdfs:label "Adjacent to Outdoor Space"@en .


###  https://w3id.org/eepsa#BadInsulatedSpace
:BadInsulatedSpace rdf:type owl:Class ;
                   rdfs:subClassOf <https://w3id.org/def/saref4bldg#BuildingSpace> ,
                                   [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                            owl:onProperty :isHighlyAffectedBy ;
                                                            owl:hasValue <https://w3id.org/measurements4eepsa#OutdoorRelativeHumidity>
                                                          ]
                                                          [ rdf:type owl:Restriction ;
                                                            owl:onProperty :isHighlyAffectedBy ;
                                                            owl:hasValue <https://w3id.org/measurements4eepsa#OutdoorTemperature>
                                                          ]
                                                        ) ;
                                     rdf:type owl:Class
                                   ] ;
                   rdfs:comment "A space with bad insulation."@en ;
                   rdfs:label "Bad Insulated Space"@en .


###  https://w3id.org/eepsa#BelowGroundLevelSpace
:BelowGroundLevelSpace rdf:type owl:Class ;
                       rdfs:subClassOf <https://w3id.org/def/saref4bldg#BuildingSpace> ;
                       rdfs:comment "A space located in a storey below ground."@en ;
                       rdfs:label "Below Ground Level Space"@en .


###  https://w3id.org/eepsa#BlindActuator
:BlindActuator rdf:type owl:Class ;
               rdfs:subClassOf <http://www.w3.org/ns/sosa/Actuator> ;
               rdfs:comment "Actuator used to put the blinds up or down"@en .


###  https://w3id.org/eepsa#DeviceRelocation
:DeviceRelocation rdf:type owl:Class ;
                  rdfs:subClassOf <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Method> ;
                  rdfs:comment """A method that consists in placing the sensing device in an adequate location. Following guides can be followed:

https://www.rmets.org/sites/default/files/pdf/guidelines/aws-guide.pdf

http://weatherfaqs.org.uk/node/124

http://www.conservationphysics.org/datalog/datlog6.php

http://www.weatherstations.co.uk/gooddata.htm"""@en ;
                  rdfs:label "Device Relocation"@en .


###  https://w3id.org/eepsa#DeviceShelter
:DeviceShelter rdf:type owl:Class ;
               rdfs:subClassOf <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Method> ;
