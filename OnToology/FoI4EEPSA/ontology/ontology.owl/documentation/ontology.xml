    <owl:DatatypeProperty rdf:about="https://w3id.org/eepsa/foi4eepsa#hasGrossPlannedArea">
        <rdfs:comment xml:lang="en">Total planned area for the building Used for programming the building (Source: IFC2x3 Property Set Definition Reference).</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/eepsa/foi4eepsa#"/>
        <rdfs:label xml:lang="en">has gross planned area</rdfs:label>
        <ns1:term_status>stable</ns1:term_status>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/eepsa/foi4eepsa#hasMarketCategory -->


    <owl:DatatypeProperty rdf:about="https://w3id.org/eepsa/foi4eepsa#hasMarketCategory">
        <rdfs:comment xml:lang="en">Category of use e.g. residential, commercial, recreation etc. (Source: IFC2x3 Property Set Definition Reference).</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/eepsa/foi4eepsa#"/>
        <rdfs:label xml:lang="en">has market category</rdfs:label>
        <ns1:term_status>stable</ns1:term_status>
    </owl:DatatypeProperty>
    


