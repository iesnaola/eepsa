    <owl:DatatypeProperty rdf:about="https://w3id.org/eepsa/foi4eepsa#hasOccupancyType">
        <rdfs:comment xml:lang="en">Occupancy type for this object. It is defined according to the presiding national building code (Source: IFC2x3 Property Set Definition Reference).</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/eepsa/foi4eepsa#"/>
        <rdfs:label xml:lang="en">has occupancy type</rdfs:label>
        <ns1:term_status>stable</ns1:term_status>
    </owl:DatatypeProperty>
    


