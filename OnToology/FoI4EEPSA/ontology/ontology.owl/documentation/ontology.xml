    <owl:DatatypeProperty rdf:about="https://w3id.org/eepsa/foi4eepsa#hasMarketSubCategory">
        <rdfs:comment xml:lang="en">Subset of category of use e.g. multi-family, 2 bedroom, low rise (Source: IFC2x3 Property Set Definition Reference).</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/eepsa/foi4eepsa#"/>
        <rdfs:label xml:lang="en">has market subcategory</rdfs:label>
        <ns1:term_status>stable</ns1:term_status>
    </owl:DatatypeProperty>
    


