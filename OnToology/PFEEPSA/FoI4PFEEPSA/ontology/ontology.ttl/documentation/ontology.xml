        <rdfs:label xml:lang="en">Farm</rdfs:label>
        <rdfs:seeAlso rdf:resource="http://aims.fao.org/aos/agrovoc/c_10743"/>
        <ns1:term_status>stable</ns1:term_status>
    </owl:Class>
    


    <!-- https://w3id.org/pfeepsa/foi4pfeepsa#PoultryHousing -->


