    <!-- https://w3id.org/pfeepsa/foi4pfeepsa#Drinker -->


    <owl:Class rdf:about="https://w3id.org/pfeepsa/foi4pfeepsa#Drinker">
        <rdfs:subClassOf rdf:resource="https://w3id.org/pfeepsa/foi4pfeepsa#AnimalHusbandryEquipment"/>
        <rdfs:comment xml:lang="en">An equipment that dispenses water for feeding purposes.</rdfs:comment>
