            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="https://w3id.org/bim4eepsa#Door"/>
        <rdfs:comment xml:lang="en">A door that is external (facing the outside of the building)</rdfs:comment>
        <rdfs:label xml:lang="en">External door</rdfs:label>
    </owl:Class>
    


