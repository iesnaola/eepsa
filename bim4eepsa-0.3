    <owl:Class rdf:about="https://w3id.org/bim4eepsa#ExternalBuildingElement">
        <rdfs:subClassOf rdf:resource="https://w3id.org/bot#Element"/>
        <rdfs:comment xml:lang="en">A building element that is external (face the outside of the building)</rdfs:comment>
        <rdfs:label xml:lang="en">External building element</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/bim4eepsa#ExternalDoor -->

    <owl:Class rdf:about="https://w3id.org/bim4eepsa#ExternalDoor">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://w3id.org/bim4eepsa#Door"/>
